   2599          /*-----------------------------------------------------------------------*/
   2600          /* Synchronize the File Object                                           */
   2601          /*-----------------------------------------------------------------------*/
   2602          
   2603          FRESULT f_sync (
   2604          	FIL *fp		/* Pointer to the file object */
   2605          )
   2606          {
   2607          	FRESULT res;
   2608          	DWORD tim;
   2609          	BYTE *dir;
   2610          
   2611          
   2612          	res = validate(fp->fs, fp->id);		/* Check validity of the object */
   2613          	if (res == FR_OK) {
   2614          		if (fp->flag & FA__WRITTEN) {	/* Has the file been written? */
   2615          #if !_FS_TINY	/* Write-back dirty buffer */
   2616          			if (fp->flag & FA__DIRTY) {
   2617          				if (disk_write(fp->fs->drv, fp->buf, fp->dsect, 1) != RES_OK)
   2618          					LEAVE_FF(fp->fs, FR_DISK_ERR);
   2619          				fp->flag &= ~FA__DIRTY;
   2620          			}
   2621          #endif
   2622          			/* Update the directory entry */
   2623          			res = move_window(fp->fs, fp->dir_sect);
   2624          			if (res == FR_OK) {
   2625          				dir = fp->dir_ptr;
   2626          				dir[DIR_Attr] |= AM_ARC;					/* Set archive bit */
   2627          				ST_DWORD(dir+DIR_FileSize, fp->fsize);		/* Update file size */
   2628          				ST_CLUST(dir, fp->sclust);					/* Update start cluster */
   2629          				tim = get_fattime();						/* Update updated time */
   2630          				ST_DWORD(dir+DIR_WrtTime, tim);
   2631          				fp->flag &= ~FA__WRITTEN;
   2632          				fp->fs->wflag = 1;
   2633          				res = sync(fp->fs);
   2634          			}
   2635          		}
   2636          	}
   2637          
   2638          	LEAVE_FF(fp->fs, res);
   2639          }
