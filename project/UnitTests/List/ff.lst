   2379          /*-----------------------------------------------------------------------*/
   2380          /* Read File                                                             */
   2381          /*-----------------------------------------------------------------------*/
   2382          
   2383          FRESULT f_read (
   2384          	FIL *fp, 		/* Pointer to the file object */
   2385          	void *buff,		/* Pointer to data buffer */
   2386          	UINT btr,		/* Number of bytes to read */
   2387          	UINT *br		/* Pointer to number of bytes read */
   2388          )
   2389          {
   2390          	FRESULT res;
   2391          	DWORD clst, sect, remain;
   2392          	UINT rcnt, cc;
   2393          	BYTE csect, *rbuff = buff;
   2394          
   2395          
   2396          	*br = 0;	/* Initialize byte counter */
   2397          
   2398          	res = validate(fp->fs, fp->id);				/* Check validity */
   2399          	if (res != FR_OK) LEAVE_FF(fp->fs, res);
   2400          	if (fp->flag & FA__ERROR)					/* Aborted file? */
   2401          		LEAVE_FF(fp->fs, FR_INT_ERR);
   2402          	if (!(fp->flag & FA_READ)) 					/* Check access mode */
   2403          		LEAVE_FF(fp->fs, FR_DENIED);
   2404          	remain = fp->fsize - fp->fptr;
   2405          	if (btr > remain) btr = (UINT)remain;		/* Truncate btr by remaining bytes */
   2406          
   2407          	for ( ;  btr;								/* Repeat until all data read */
   2408          		rbuff += rcnt, fp->fptr += rcnt, *br += rcnt, btr -= rcnt) {
   2409          		if ((fp->fptr % SS(fp->fs)) == 0) {		/* On the sector boundary? */
   2410          			csect = (BYTE)(fp->fptr / SS(fp->fs) & (fp->fs->csize - 1));	/* Sector offset in the cluster */
   2411          			if (!csect) {						/* On the cluster boundary? */
   2412          				if (fp->fptr == 0) {			/* On the top of the file? */
   2413          					clst = fp->sclust;			/* Follow from the origin */
   2414          				} else {						/* Middle or end of the file */
   2415          #if _USE_FASTSEEK
   2416          					if (fp->cltbl)
   2417          						clst = clmt_clust(fp, fp->fptr);	/* Get cluster# from the CLMT */
   2418          					else
   2419          #endif
   2420          						clst = get_fat(fp->fs, fp->clust);	/* Follow cluster chain on the FAT */
   2421          				}
   2422          				if (clst < 2) ABORT(fp->fs, FR_INT_ERR);
   2423          				if (clst == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   2424          				fp->clust = clst;				/* Update current cluster */
   2425          			}
   2426          			sect = clust2sect(fp->fs, fp->clust);	/* Get current sector */
   2427          			if (!sect) ABORT(fp->fs, FR_INT_ERR);
   2428          			sect += csect;
   2429          			cc = btr / SS(fp->fs);				/* When remaining bytes >= sector size, */
   2430          			if (cc) {							/* Read maximum contiguous sectors directly */
   2431          				if (csect + cc > fp->fs->csize)	/* Clip at cluster boundary */
   2432          					cc = fp->fs->csize - csect;
   2433          				if (disk_read(fp->fs->drv, rbuff, sect, (BYTE)cc) != RES_OK)
   2434          					ABORT(fp->fs, FR_DISK_ERR);
   2435          #if !_FS_READONLY && _FS_MINIMIZE <= 2			/* Replace one of the read sectors with cached data if it contains a dirty sector */
   2436          #if _FS_TINY
   2437          				if (fp->fs->wflag && fp->fs->winsect - sect < cc)
   2438          					mem_cpy(rbuff + ((fp->fs->winsect - sect) * SS(fp->fs)), fp->fs->win, SS(fp->fs));
   2439          #else
   2440          				if ((fp->flag & FA__DIRTY) && fp->dsect - sect < cc)
   2441          					mem_cpy(rbuff + ((fp->dsect - sect) * SS(fp->fs)), fp->buf, SS(fp->fs));
   2442          #endif
   2443          #endif
   2444          				rcnt = SS(fp->fs) * cc;			/* Number of bytes transferred */
   2445          				continue;
   2446          			}
   2447          #if !_FS_TINY
   2448          			if (fp->dsect != sect) {			/* Load data sector if not in cache */
   2449          #if !_FS_READONLY
   2450          				if (fp->flag & FA__DIRTY) {		/* Write-back dirty sector cache */
   2451          					if (disk_write(fp->fs->drv, fp->buf, fp->dsect, 1) != RES_OK)
   2452          						ABORT(fp->fs, FR_DISK_ERR);
   2453          					fp->flag &= ~FA__DIRTY;
   2454          				}
   2455          #endif
   2456          				if (disk_read(fp->fs->drv, fp->buf, sect, 1) != RES_OK)	/* Fill sector cache */
   2457          					ABORT(fp->fs, FR_DISK_ERR);
   2458          			}
   2459          #endif
   2460          			fp->dsect = sect;
   2461          		}
   2462          		rcnt = SS(fp->fs) - (fp->fptr % SS(fp->fs));	/* Get partial sector data from sector buffer */
   2463          		if (rcnt > btr) rcnt = btr;
   2464          #if _FS_TINY
   2465          		if (move_window(fp->fs, fp->dsect))		/* Move sector window */
   2466          			ABORT(fp->fs, FR_DISK_ERR);
   2467          		mem_cpy(rbuff, &fp->fs->win[fp->fptr % SS(fp->fs)], rcnt);	/* Pick partial sector */
   2468          #else
   2469          		mem_cpy(rbuff, &fp->buf[fp->fptr % SS(fp->fs)], rcnt);	/* Pick partial sector */
   2470          #endif
   2471          	}
   2472          
   2473          	LEAVE_FF(fp->fs, FR_OK);
   2474          }
   2475          
   2476          
   2477          
   2478          
   2479          #if !_FS_READONLY
   2480          /*-----------------------------------------------------------------------*/
   2481          /* Write File                                                            */
   2482          /*-----------------------------------------------------------------------*/
   2483          
   2484          FRESULT f_write (
   2485          	FIL *fp,			/* Pointer to the file object */
   2486          	const void *buff,	/* Pointer to the data to be written */
   2487          	UINT btw,			/* Number of bytes to write */
   2488          	UINT *bw			/* Pointer to number of bytes written */
   2489          )
   2490          {
   2491          	FRESULT res;
   2492          	DWORD clst, sect;
   2493          	UINT wcnt, cc;
   2494          	const BYTE *wbuff = buff;
   2495          	BYTE csect;
   2496          
   2497          
   2498          	*bw = 0;	/* Initialize byte counter */
   2499          
   2500          	res = validate(fp->fs, fp->id);			/* Check validity */
   2501          	if (res != FR_OK) LEAVE_FF(fp->fs, res);
   2502          	if (fp->flag & FA__ERROR)				/* Aborted file? */
   2503          		LEAVE_FF(fp->fs, FR_INT_ERR);
   2504          	if (!(fp->flag & FA_WRITE))				/* Check access mode */
   2505          		LEAVE_FF(fp->fs, FR_DENIED);
   2506          	if ((DWORD)(fp->fsize + btw) < fp->fsize) btw = 0;	/* File size cannot reach 4GB */
   2507          
   2508          	for ( ;  btw;							/* Repeat until all data written */
   2509          		wbuff += wcnt, fp->fptr += wcnt, *bw += wcnt, btw -= wcnt) {
   2510          		if ((fp->fptr % SS(fp->fs)) == 0) {	/* On the sector boundary? */
   2511          			csect = (BYTE)(fp->fptr / SS(fp->fs) & (fp->fs->csize - 1));	/* Sector offset in the cluster */
   2512          			if (!csect) {					/* On the cluster boundary? */
   2513          				if (fp->fptr == 0) {		/* On the top of the file? */
   2514          					clst = fp->sclust;		/* Follow from the origin */
   2515          					if (clst == 0)			/* When no cluster is allocated, */
   2516          						fp->sclust = clst = create_chain(fp->fs, 0);	/* Create a new cluster chain */
   2517          				} else {					/* Middle or end of the file */
   2518          #if _USE_FASTSEEK
   2519          					if (fp->cltbl)
   2520          						clst = clmt_clust(fp, fp->fptr);	/* Get cluster# from the CLMT */
   2521          					else
   2522          #endif
   2523          						clst = create_chain(fp->fs, fp->clust);	/* Follow or stretch cluster chain on the FAT */
   2524          				}
   2525          				if (clst == 0) break;		/* Could not allocate a new cluster (disk full) */
   2526          				if (clst == 1) ABORT(fp->fs, FR_INT_ERR);
   2527          				if (clst == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   2528          				fp->clust = clst;			/* Update current cluster */
   2529          			}
   2530          #if _FS_TINY
   2531          			if (fp->fs->winsect == fp->dsect && move_window(fp->fs, 0))	/* Write-back sector cache */
   2532          				ABORT(fp->fs, FR_DISK_ERR);
   2533          #else
   2534          			if (fp->flag & FA__DIRTY) {		/* Write-back sector cache */
   2535          				if (disk_write(fp->fs->drv, fp->buf, fp->dsect, 1) != RES_OK)
   2536          					ABORT(fp->fs, FR_DISK_ERR);
   2537          				fp->flag &= ~FA__DIRTY;
   2538          			}
   2539          #endif
   2540          			sect = clust2sect(fp->fs, fp->clust);	/* Get current sector */
   2541          			if (!sect) ABORT(fp->fs, FR_INT_ERR);
   2542          			sect += csect;
   2543          			cc = btw / SS(fp->fs);			/* When remaining bytes >= sector size, */
   2544          			if (cc) {						/* Write maximum contiguous sectors directly */
   2545          				if (csect + cc > fp->fs->csize)	/* Clip at cluster boundary */
   2546          					cc = fp->fs->csize - csect;
   2547          				if (disk_write(fp->fs->drv, wbuff, sect, (BYTE)cc) != RES_OK)
   2548          					ABORT(fp->fs, FR_DISK_ERR);
   2549          #if _FS_TINY
   2550          				if (fp->fs->winsect - sect < cc) {	/* Refill sector cache if it gets invalidated by the direct write */
   2551          					mem_cpy(fp->fs->win, wbuff + ((fp->fs->winsect - sect) * SS(fp->fs)), SS(fp->fs));
   2552          					fp->fs->wflag = 0;
   2553          				}
   2554          #else
   2555          				if (fp->dsect - sect < cc) { /* Refill sector cache if it gets invalidated by the direct write */
   2556          					mem_cpy(fp->buf, wbuff + ((fp->dsect - sect) * SS(fp->fs)), SS(fp->fs));
   2557          					fp->flag &= ~FA__DIRTY;
   2558          				}
   2559          #endif
   2560          				wcnt = SS(fp->fs) * cc;		/* Number of bytes transferred */
   2561          				continue;
   2562          			}
   2563          #if _FS_TINY
   2564          			if (fp->fptr >= fp->fsize) {	/* Avoid silly cache filling at growing edge */
   2565          				if (move_window(fp->fs, 0)) ABORT(fp->fs, FR_DISK_ERR);
   2566          				fp->fs->winsect = sect;
   2567          			}
   2568          #else
   2569          			if (fp->dsect != sect) {		/* Fill sector cache with file data */
   2570          				if (fp->fptr < fp->fsize &&
   2571          					disk_read(fp->fs->drv, fp->buf, sect, 1) != RES_OK)
   2572          						ABORT(fp->fs, FR_DISK_ERR);
   2573          			}
   2574          #endif
   2575          			fp->dsect = sect;
   2576          		}
   2577          		wcnt = SS(fp->fs) - (fp->fptr % SS(fp->fs));/* Put partial sector into file I/O buffer */
   2578          		if (wcnt > btw) wcnt = btw;
   2579          #if _FS_TINY
   2580          		if (move_window(fp->fs, fp->dsect))	/* Move sector window */
   2581          			ABORT(fp->fs, FR_DISK_ERR);
   2582          		mem_cpy(&fp->fs->win[fp->fptr % SS(fp->fs)], wbuff, wcnt);	/* Fit partial sector */
   2583          		fp->fs->wflag = 1;
   2584          #else
   2585          		mem_cpy(&fp->buf[fp->fptr % SS(fp->fs)], wbuff, wcnt);	/* Fit partial sector */
   2586          		fp->flag |= FA__DIRTY;
   2587          #endif
   2588          	}
   2589          
   2590          	if (fp->fptr > fp->fsize) fp->fsize = fp->fptr;	/* Update file size if needed */
   2591          	fp->flag |= FA__WRITTEN;						/* Set file change flag */
   2592          
   2593          	LEAVE_FF(fp->fs, FR_OK);
   2594          }
