   3926          /* Put a character to the file                                           */
   3927          /*-----------------------------------------------------------------------*/
   3928          int f_putc (
   3929          	TCHAR c,	/* A character to be output */
   3930          	FIL* fil	/* Pointer to the file object */
   3931          )
   3932          {
   3933          	UINT bw, btw;
   3934          	BYTE s[3];
   3935          
   3936          
   3937          #if _USE_STRFUNC >= 2
   3938          	if (c == '\n') f_putc ('\r', fil);	/* LF -> CRLF conversion */
   3939          #endif
   3940          
   3941          #if _LFN_UNICODE	/* Write the character in UTF-8 encoding */
   3942          	if (c < 0x80) {			/* 7-bit */
   3943          		s[0] = (BYTE)c;
   3944          		btw = 1;
   3945          	} else {
   3946          		if (c < 0x800) {	/* 11-bit */
   3947          			s[0] = (BYTE)(0xC0 | (c >> 6));
   3948          			s[1] = (BYTE)(0x80 | (c & 0x3F));
   3949          			btw = 2;
   3950          		} else {			/* 16-bit */
   3951          			s[0] = (BYTE)(0xE0 | (c >> 12));
   3952          			s[1] = (BYTE)(0x80 | ((c >> 6) & 0x3F));
   3953          			s[2] = (BYTE)(0x80 | (c & 0x3F));
   3954          			btw = 3;
   3955          		}
   3956          	}
   3957          #else				/* Write the character without conversion */
   3958          	s[0] = (BYTE)c;
   3959          	btw = 1;
   3960          #endif
   3961          	f_write(fil, s, btw, &bw);		/* Write the char to the file */
   3962          	return (bw == btw) ? 1 : EOF;	/* Return the result */
   3963          }
