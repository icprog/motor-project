   3034          				res = dir_next(dj, 0);		/* Increment index for next */
   3035          				if (res == FR_NO_FILE) {
   3036          					dj->sect = 0;
   3037          					res = FR_OK;
   3038          				}
   3039          			}
   3040          			FREE_BUF();
   3041          		}
   3042          	}
   3043          
   3044          	LEAVE_FF(dj->fs, res);
   3045          }
