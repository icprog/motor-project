   1353          
   1354          /*-----------------------------------------------------------------------*/
   1355          /* Directory handling - Find an object in the directory                  */
   1356          /*-----------------------------------------------------------------------*/
   1357          
   1358          static
   1359          FRESULT dir_find (
   1360          	DIR *dj			/* Pointer to the directory object linked to the file name */
   1361          )
   1362          {
   1363          	FRESULT res;
   1364          	BYTE c, *dir;
   1365          #if _USE_LFN
   1366          	BYTE a, ord, sum;
   1367          #endif
   1368          
   1369          	res = dir_sdi(dj, 0);			/* Rewind directory object */
   1370          	if (res != FR_OK) return res;
   1371          
   1372          #if _USE_LFN
   1373          	ord = sum = 0xFF;
   1374          #endif
   1375          	do {
   1376          		res = move_window(dj->fs, dj->sect);
   1377          		if (res != FR_OK) break;
   1378          		dir = dj->dir;					/* Ptr to the directory entry of current index */
   1379          		c = dir[DIR_Name];
   1380          		if (c == 0) { res = FR_NO_FILE; break; }	/* Reached to end of table */
   1381          #if _USE_LFN	/* LFN configuration */
   1382          		a = dir[DIR_Attr] & AM_MASK;
   1383          		if (c == DDE || ((a & AM_VOL) && a != AM_LFN)) {	/* An entry without valid data */
   1384          			ord = 0xFF;
   1385          		} else {
   1386          			if (a == AM_LFN) {			/* An LFN entry is found */
   1387          				if (dj->lfn) {
   1388          					if (c & LLE) {		/* Is it start of LFN sequence? */
   1389          						sum = dir[LDIR_Chksum];
   1390          						c &= ~LLE; ord = c;	/* LFN start order */
   1391          						dj->lfn_idx = dj->index;
   1392          					}
   1393          					/* Check validity of the LFN entry and compare it with given name */
   1394          					ord = (c == ord && sum == dir[LDIR_Chksum] && cmp_lfn(dj->lfn, dir)) ? ord - 1 : 0xFF;
   1395          				}
   1396          			} else {					/* An SFN entry is found */
   1397          				if (!ord && sum == sum_sfn(dir)) break;	/* LFN matched? */
   1398          				ord = 0xFF; dj->lfn_idx = 0xFFFF;	/* Reset LFN sequence */
   1399          				if (!(dj->fn[NS] & NS_LOSS) && !mem_cmp(dir, dj->fn, 11)) break;	/* SFN matched? */
   1400          			}
   1401          		}
   1402          #else		/* Non LFN configuration */
   1403          		if (!(dir[DIR_Attr] & AM_VOL) && !mem_cmp(dir, dj->fn, 11)) /* Is it a valid entry? */
   1404          			break;
   1405          #endif
   1406          		res = dir_next(dj, 0);		/* Next entry */
   1407          	} while (res == FR_OK);
   1408          
   1409          	return res;
   1410          }
   1411          
