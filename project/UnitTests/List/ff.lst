   2734          
   2735          #if _FS_RPATH >= 2
   2736          FRESULT f_getcwd (
   2737          	TCHAR *path,	/* Pointer to the directory path */
   2738          	UINT sz_path	/* Size of path */
   2739          )
   2740          {
   2741          	FRESULT res;
   2742          	DIR dj;
   2743          	UINT i, n;
   2744          	DWORD ccl;
   2745          	TCHAR *tp;
   2746          	FILINFO fno;
   2747          	DEF_NAMEBUF;
   2748          
   2749          
   2750          	*path = 0;
   2751          	res = chk_mounted((const TCHAR**)&path, &dj.fs, 0);	/* Get current volume */
   2752          	if (res == FR_OK) {
   2753          		INIT_BUF(dj);
   2754          		i = sz_path;		/* Bottom of buffer (dir stack base) */
   2755          		dj.sclust = dj.fs->cdir;			/* Start to follow upper dir from current dir */
   2756          		while ((ccl = dj.sclust) != 0) {	/* Repeat while current dir is a sub-dir */
   2757          			res = dir_sdi(&dj, 1);			/* Get parent dir */
   2758          			if (res != FR_OK) break;
   2759          			res = dir_read(&dj);
   2760          			if (res != FR_OK) break;
   2761          			dj.sclust = LD_CLUST(dj.dir);	/* Goto parent dir */
   2762          			res = dir_sdi(&dj, 0);
   2763          			if (res != FR_OK) break;
   2764          			do {							/* Find the entry links to the child dir */
   2765          				res = dir_read(&dj);
   2766          				if (res != FR_OK) break;
   2767          				if (ccl == LD_CLUST(dj.dir)) break;	/* Found the entry */
   2768          				res = dir_next(&dj, 0);
   2769          			} while (res == FR_OK);
   2770          			if (res == FR_NO_FILE) res = FR_INT_ERR;/* It cannot be 'not found'. */
   2771          			if (res != FR_OK) break;
   2772          #if _USE_LFN
   2773          			fno.lfname = path;
   2774          			fno.lfsize = i;
   2775          #endif
   2776          			get_fileinfo(&dj, &fno);		/* Get the dir name and push it to the buffer */
   2777          			tp = fno.fname;
   2778          			if (_USE_LFN && *path) tp = path;
   2779          			for (n = 0; tp[n]; n++) ;
   2780          			if (i < n + 3) {
   2781          				res = FR_NOT_ENOUGH_CORE; break;
   2782          			}
   2783          			while (n) path[--i] = tp[--n];
   2784          			path[--i] = '/';
   2785          		}
   2786          		tp = path;
   2787          		if (res == FR_OK) {
   2788          			*tp++ = '0' + CurrVol;			/* Put drive number */
   2789          			*tp++ = ':';
   2790          			if (i == sz_path) {				/* Root-dir */
   2791          				*tp++ = '/';
   2792          			} else {						/* Sub-dir */
   2793          				do		/* Add stacked path str */
   2794          					*tp++ = path[i++];
   2795          				while (i < sz_path);
   2796          			}
   2797          		}
   2798          		*tp = 0;
   2799          		FREE_BUF();
   2800          	}
   2801          
   2802          	LEAVE_FF(dj.fs, res);
   2803          }
   2804          #endif /* _FS_RPATH >= 2 */
   2805          #endif /* _FS_RPATH >= 1 */
