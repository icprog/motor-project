   1988          /* Load a sector and check if it is an FAT Volume Boot Record            */
   1989          /*-----------------------------------------------------------------------*/
   1990          
   1991          static
   1992          BYTE check_fs (	/* 0:FAT-VBR, 1:Valid BR but not FAT, 2:Not a BR, 3:Disk error */
   1993          	FATFS *fs,	/* File system object */
   1994          	DWORD sect	/* Sector# (lba) to check if it is an FAT boot record or not */
   1995          )
   1996          {
   1997          	if (disk_read(fs->drv, fs->win, sect, 1) != RES_OK)	/* Load boot record */
   1998          		return 3;
   1999          	if (LD_WORD(&fs->win[BS_55AA]) != 0xAA55)		/* Check record signature (always placed at offset 510 even if the sector size is >512) */
   2000          		return 2;
   2001          
   2002          	if ((LD_DWORD(&fs->win[BS_FilSysType]) & 0xFFFFFF) == 0x544146)	/* Check "FAT" string */
   2003          		return 0;
   2004          	if ((LD_DWORD(&fs->win[BS_FilSysType32]) & 0xFFFFFF) == 0x544146)
   2005          		return 0;
   2006          
   2007          	return 1;
   2008          }
