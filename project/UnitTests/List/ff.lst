   3148          /*-----------------------------------------------------------------------*/
   3149          /* Truncate File                                                         */
   3150          /*-----------------------------------------------------------------------*/
   3151          
   3152          FRESULT f_truncate (
   3153          	FIL *fp		/* Pointer to the file object */
   3154          )
   3155          {
   3156          	FRESULT res;
   3157          	DWORD ncl;
   3158          
   3159          
   3160          	res = validate(fp->fs, fp->id);		/* Check validity of the object */
   3161          	if (res == FR_OK) {
   3162          		if (fp->flag & FA__ERROR) {			/* Check abort flag */
   3163          			res = FR_INT_ERR;
   3164          		} else {
   3165          			if (!(fp->flag & FA_WRITE))		/* Check access mode */
   3166          				res = FR_DENIED;
   3167          		}
   3168          	}
   3169          	if (res == FR_OK) {
   3170          		if (fp->fsize > fp->fptr) {
   3171          			fp->fsize = fp->fptr;	/* Set file size to current R/W point */
   3172          			fp->flag |= FA__WRITTEN;
   3173          			if (fp->fptr == 0) {	/* When set file size to zero, remove entire cluster chain */
   3174          				res = remove_chain(fp->fs, fp->sclust);
   3175          				fp->sclust = 0;
   3176          			} else {				/* When truncate a part of the file, remove remaining clusters */
   3177          				ncl = get_fat(fp->fs, fp->clust);
   3178          				res = FR_OK;
   3179          				if (ncl == 0xFFFFFFFF) res = FR_DISK_ERR;
   3180          				if (ncl == 1) res = FR_INT_ERR;
   3181          				if (res == FR_OK && ncl < fp->fs->n_fatent) {
   3182          					res = put_fat(fp->fs, fp->clust, 0x0FFFFFFF);
   3183          					if (res == FR_OK) res = remove_chain(fp->fs, ncl);
   3184          				}
   3185          			}
   3186          		}
   3187          		if (res != FR_OK) fp->flag |= FA__ERROR;
   3188          	}
   3189          
   3190          	LEAVE_FF(fp->fs, res);
   3191          }
