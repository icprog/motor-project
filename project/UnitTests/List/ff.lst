   3196          /*-----------------------------------------------------------------------*/
   3197          /* Delete a File or Directory                                            */
   3198          /*-----------------------------------------------------------------------*/
   3199          
   3200          FRESULT f_unlink (
   3201          	const TCHAR *path		/* Pointer to the file or directory path */
   3202          )
   3203          {
   3204          	FRESULT res;
   3205          	DIR dj, sdj;
   3206          	BYTE *dir;
   3207          	DWORD dclst;
   3208          	DEF_NAMEBUF;
   3209          
   3210          
   3211          	res = chk_mounted(&path, &dj.fs, 1);
   3212          	if (res == FR_OK) {
   3213          		INIT_BUF(dj);
   3214          		res = follow_path(&dj, path);		/* Follow the file path */
   3215          		if (_FS_RPATH && res == FR_OK && (dj.fn[NS] & NS_DOT))
   3216          			res = FR_INVALID_NAME;			/* Cannot remove dot entry */
   3217          #if _FS_SHARE
   3218          		if (res == FR_OK) res = chk_lock(&dj, 2);	/* Cannot remove open file */
   3219          #endif
   3220          		if (res == FR_OK) {					/* The object is accessible */
   3221          			dir = dj.dir;
   3222          			if (!dir) {
   3223          				res = FR_INVALID_NAME;		/* Cannot remove the start directory */
   3224          			} else {
   3225          				if (dir[DIR_Attr] & AM_RDO)
   3226          					res = FR_DENIED;		/* Cannot remove R/O object */
   3227          			}
   3228          			dclst = LD_CLUST(dir);
   3229          			if (res == FR_OK && (dir[DIR_Attr] & AM_DIR)) {	/* Is it a sub-dir? */
   3230          				if (dclst < 2) {
   3231          					res = FR_INT_ERR;
   3232          				} else {
   3233          					mem_cpy(&sdj, &dj, sizeof(DIR));	/* Check if the sub-dir is empty or not */
   3234          					sdj.sclust = dclst;
   3235          					res = dir_sdi(&sdj, 2);		/* Exclude dot entries */
   3236          					if (res == FR_OK) {
   3237          						res = dir_read(&sdj);
   3238          						if (res == FR_OK			/* Not empty dir */
   3239          #if _FS_RPATH
   3240          						|| dclst == sdj.fs->cdir	/* Current dir */
   3241          #endif
   3242          						) res = FR_DENIED;
   3243          						if (res == FR_NO_FILE) res = FR_OK;	/* Empty */
   3244          					}
   3245          				}
   3246          			}
   3247          			if (res == FR_OK) {
   3248          				res = dir_remove(&dj);		/* Remove the directory entry */
   3249          				if (res == FR_OK) {
   3250          					if (dclst)				/* Remove the cluster chain if exist */
   3251          						res = remove_chain(dj.fs, dclst);
   3252          					if (res == FR_OK) res = sync(dj.fs);
   3253          				}
   3254          			}
   3255          		}
   3256          		FREE_BUF();
   3257          	}
   3258          	LEAVE_FF(dj.fs, res);
   3259          }
