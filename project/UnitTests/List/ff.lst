   1334          /*-----------------------------------------------------------------------*/
   1335          /* Calculate sum of an SFN                                               */
   1336          /*-----------------------------------------------------------------------*/
   1337          #if _USE_LFN
   1338          static
   1339          BYTE sum_sfn (
   1340          	const BYTE *dir		/* Ptr to directory entry */
   1341          )
   1342          {
   1343          	BYTE sum = 0;
   1344          	UINT n = 11;
   1345          
   1346          	do sum = (sum >> 1) + (sum << 7) + *dir++; while (--n);
   1347          	return sum;
   1348          }
   1349          #endif
