   2013          /*-----------------------------------------------------------------------*/
   2014          /* Check if the file system object is valid or not                       */
   2015          /*-----------------------------------------------------------------------*/
   2016          
   2017          static
   2018          FRESULT chk_mounted (	/* FR_OK(0): successful, !=0: any error occurred */
   2019          	const TCHAR **path,	/* Pointer to pointer to the path name (drive number) */
   2020          	FATFS **rfs,		/* Pointer to pointer to the found file system object */
   2021          	BYTE chk_wp			/* !=0: Check media write protection for write access */
   2022          )
   2023          {
   2024          	BYTE fmt, b, pi, *tbl;
   2025          	UINT vol;
   2026          	DSTATUS stat;
   2027          	DWORD bsect, fasize, tsect, sysect, nclst, szbfat;
   2028          	WORD nrsv;
   2029          	const TCHAR *p = *path;
   2030          	FATFS *fs;
   2031          
   2032          	/* Get logical drive number from the path name */
   2033          	vol = p[0] - '0';					/* Is there a drive number? */
   2034          	if (vol <= 9 && p[1] == ':') {		/* Found a drive number, get and strip it */
   2035          		p += 2; *path = p;				/* Return pointer to the path name */
   2036          	} else {							/* No drive number is given */
   2037          #if _FS_RPATH
   2038          		vol = CurrVol;					/* Use current drive */
   2039          #else
   2040          		vol = 0;						/* Use drive 0 */
   2041          #endif
   2042          	}
   2043          
   2044          	/* Check if the file system object is valid or not */
   2045          	if (vol >= _VOLUMES) 				/* Is the drive number valid? */
   2046          		return FR_INVALID_DRIVE;
   2047          	*rfs = fs = FatFs[vol];				/* Return pointer to the corresponding file system object */
   2048          	if (!fs) return FR_NOT_ENABLED;		/* Is the file system object available? */
   2049          
   2050          	ENTER_FF(fs);						/* Lock file system */
   2051          
   2052          	if (fs->fs_type) {					/* If the logical drive has been mounted */
   2053          		stat = disk_status(fs->drv);
   2054          		if (!(stat & STA_NOINIT)) {		/* and the physical drive is kept initialized (has not been changed), */
   2055          			if (!_FS_READONLY && chk_wp && (stat & STA_PROTECT))	/* Check write protection if needed */
   2056          				return FR_WRITE_PROTECTED;
   2057          			return FR_OK;				/* The file system object is valid */
   2058          		}
   2059          	}
   2060          
   2061          	/* The file system object is not valid. */
   2062          	/* Following code attempts to mount the volume. (analyze BPB and initialize the fs object) */
   2063          
   2064          	fs->fs_type = 0;					/* Clear the file system object */
   2065          	fs->drv = LD2PD(vol);				/* Bind the logical drive and a physical drive */
   2066          	stat = disk_initialize(fs->drv);	/* Initialize low level disk I/O layer */
   2067          	if (stat & STA_NOINIT)				/* Check if the initialization succeeded */
   2068          		return FR_NOT_READY;			/* Failed to initialize due to no media or hard error */
   2069          	if (!_FS_READONLY && chk_wp && (stat & STA_PROTECT))	/* Check disk write protection if needed */
   2070          		return FR_WRITE_PROTECTED;
   2071          #if _MAX_SS != 512						/* Get disk sector size (variable sector size cfg only) */
   2072          	if (disk_ioctl(fs->drv, GET_SECTOR_SIZE, &fs->ssize) != RES_OK)
   2073          		return FR_DISK_ERR;
   2074          #endif
   2075          	/* Search FAT partition on the drive. Supports only generic partitionings, FDISK and SFD. */
   2076          	fmt = check_fs(fs, bsect = 0);		/* Load sector 0 and check if it is an FAT-VBR (in SFD) */
   2077          	if (LD2PT(vol) && !fmt) fmt = 1;	/* Force non-SFD if the volume is forced partition */
   2078          	if (fmt == 1) {						/* Not an FAT-VBR, the physical drive can be partitioned */
   2079          		/* Check the partition listed in the partition table */
   2080          		pi = LD2PT(vol);
   2081          		if (pi) pi--;
   2082          		tbl = &fs->win[MBR_Table + pi * SZ_PTE];/* Partition table */
   2083          		if (tbl[4]) {						/* Is the partition existing? */
   2084          			bsect = LD_DWORD(&tbl[8]);		/* Partition offset in LBA */
   2085          			fmt = check_fs(fs, bsect);		/* Check the partition */
   2086          		}
   2087          	}
   2088          	if (fmt == 3) return FR_DISK_ERR;
   2089          	if (fmt) return FR_NO_FILESYSTEM;		/* No FAT volume is found */
   2090          
   2091          	/* An FAT volume is found. Following code initializes the file system object */
   2092          
   2093          	if (LD_WORD(fs->win+BPB_BytsPerSec) != SS(fs))		/* (BPB_BytsPerSec must be equal to the physical sector size) */
   2094          		return FR_NO_FILESYSTEM;
   2095          
   2096          	fasize = LD_WORD(fs->win+BPB_FATSz16);				/* Number of sectors per FAT */
   2097          	if (!fasize) fasize = LD_DWORD(fs->win+BPB_FATSz32);
   2098          	fs->fsize = fasize;
   2099          
   2100          	fs->n_fats = b = fs->win[BPB_NumFATs];				/* Number of FAT copies */
   2101          	if (b != 1 && b != 2) return FR_NO_FILESYSTEM;		/* (Must be 1 or 2) */
   2102          	fasize *= b;										/* Number of sectors for FAT area */
   2103          
   2104          	fs->csize = b = fs->win[BPB_SecPerClus];			/* Number of sectors per cluster */
   2105          	if (!b || (b & (b - 1))) return FR_NO_FILESYSTEM;	/* (Must be power of 2) */
   2106          
   2107          	fs->n_rootdir = LD_WORD(fs->win+BPB_RootEntCnt);	/* Number of root directory entries */
   2108          	if (fs->n_rootdir % (SS(fs) / SZ_DIR)) return FR_NO_FILESYSTEM;	/* (BPB_RootEntCnt must be sector aligned) */
   2109          
   2110          	tsect = LD_WORD(fs->win+BPB_TotSec16);				/* Number of sectors on the volume */
   2111          	if (!tsect) tsect = LD_DWORD(fs->win+BPB_TotSec32);
   2112          
   2113          	nrsv = LD_WORD(fs->win+BPB_RsvdSecCnt);				/* Number of reserved sectors */
   2114          	if (!nrsv) return FR_NO_FILESYSTEM;					/* (BPB_RsvdSecCnt must not be 0) */
   2115          
   2116          	/* Determine the FAT sub type */
   2117          	sysect = nrsv + fasize + fs->n_rootdir / (SS(fs) / SZ_DIR);	/* RSV+FAT+DIR */
   2118          	if (tsect < sysect) return FR_NO_FILESYSTEM;		/* (Invalid volume size) */
   2119          	nclst = (tsect - sysect) / fs->csize;				/* Number of clusters */
   2120          	if (!nclst) return FR_NO_FILESYSTEM;				/* (Invalid volume size) */
   2121          	fmt = FS_FAT12;
   2122          	if (nclst >= MIN_FAT16) fmt = FS_FAT16;
   2123          	if (nclst >= MIN_FAT32) fmt = FS_FAT32;
   2124          
   2125          	/* Boundaries and Limits */
   2126          	fs->n_fatent = nclst + 2;							/* Number of FAT entries */
   2127          	fs->database = bsect + sysect;						/* Data start sector */
   2128          	fs->fatbase = bsect + nrsv; 						/* FAT start sector */
   2129          	if (fmt == FS_FAT32) {
   2130          		if (fs->n_rootdir) return FR_NO_FILESYSTEM;		/* (BPB_RootEntCnt must be 0) */
   2131          		fs->dirbase = LD_DWORD(fs->win+BPB_RootClus);	/* Root directory start cluster */
   2132          		szbfat = fs->n_fatent * 4;						/* (Required FAT size) */
   2133          	} else {
   2134          		if (!fs->n_rootdir)	return FR_NO_FILESYSTEM;	/* (BPB_RootEntCnt must not be 0) */
   2135          		fs->dirbase = fs->fatbase + fasize;				/* Root directory start sector */
   2136          		szbfat = (fmt == FS_FAT16) ?					/* (Required FAT size) */
   2137          			fs->n_fatent * 2 : fs->n_fatent * 3 / 2 + (fs->n_fatent & 1);
   2138          	}
   2139          	if (fs->fsize < (szbfat + (SS(fs) - 1)) / SS(fs))	/* (BPB_FATSz must not be less than required) */
   2140          		return FR_NO_FILESYSTEM;
   2141          
   2142          #if !_FS_READONLY
   2143          	/* Initialize cluster allocation information */
   2144          	fs->free_clust = 0xFFFFFFFF;
   2145          	fs->last_clust = 0;
   2146          
   2147          	/* Get fsinfo if available */
   2148          	if (fmt == FS_FAT32) {
   2149          	 	fs->fsi_flag = 0;
   2150          		fs->fsi_sector = bsect + LD_WORD(fs->win+BPB_FSInfo);
   2151          		if (disk_read(fs->drv, fs->win, fs->fsi_sector, 1) == RES_OK &&
   2152          			LD_WORD(fs->win+BS_55AA) == 0xAA55 &&
   2153          			LD_DWORD(fs->win+FSI_LeadSig) == 0x41615252 &&
   2154          			LD_DWORD(fs->win+FSI_StrucSig) == 0x61417272) {
   2155          				fs->last_clust = LD_DWORD(fs->win+FSI_Nxt_Free);
   2156          				fs->free_clust = LD_DWORD(fs->win+FSI_Free_Count);
   2157          		}
   2158          	}
   2159          #endif
   2160          	fs->fs_type = fmt;		/* FAT sub-type */
   2161          	fs->id = ++Fsid;		/* File system mount ID */
   2162          	fs->winsect = 0;		/* Invalidate sector cache */
   2163          	fs->wflag = 0;
   2164          #if _FS_RPATH
   2165          	fs->cdir = 0;			/* Current directory (root dir) */
   2166          #endif
   2167          #if _FS_SHARE				/* Clear file lock semaphores */
   2168          	clear_lock(fs);
   2169          #endif
   2170          
   2171          	return FR_OK;
   2172          }
