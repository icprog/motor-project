   1414          
   1415          /*-----------------------------------------------------------------------*/
   1416          /* Read an object from the directory                                     */
   1417          /*-----------------------------------------------------------------------*/
   1418          #if _FS_MINIMIZE <= 1
   1419          static
   1420          FRESULT dir_read (
   1421          	DIR *dj			/* Pointer to the directory object that pointing the entry to be read */
   1422          )
   1423          {
   1424          	FRESULT res;
   1425          	BYTE c, *dir;
   1426          #if _USE_LFN
   1427          	BYTE a, ord = 0xFF, sum = 0xFF;
   1428          #endif
   1429          
   1430          	res = FR_NO_FILE;
   1431          	while (dj->sect) {
   1432          		res = move_window(dj->fs, dj->sect);
   1433          		if (res != FR_OK) break;
   1434          		dir = dj->dir;					/* Ptr to the directory entry of current index */
   1435          		c = dir[DIR_Name];
   1436          		if (c == 0) { res = FR_NO_FILE; break; }	/* Reached to end of table */
   1437          #if _USE_LFN	/* LFN configuration */
   1438          		a = dir[DIR_Attr] & AM_MASK;
   1439          		if (c == DDE || (!_FS_RPATH && c == '.') || ((a & AM_VOL) && a != AM_LFN)) {	/* An entry without valid data */
   1440          			ord = 0xFF;
   1441          		} else {
   1442          			if (a == AM_LFN) {			/* An LFN entry is found */
   1443          				if (c & LLE) {			/* Is it start of LFN sequence? */
   1444          					sum = dir[LDIR_Chksum];
   1445          					c &= ~LLE; ord = c;
   1446          					dj->lfn_idx = dj->index;
   1447          				}
   1448          				/* Check LFN validity and capture it */
   1449          				ord = (c == ord && sum == dir[LDIR_Chksum] && pick_lfn(dj->lfn, dir)) ? ord - 1 : 0xFF;
   1450          			} else {					/* An SFN entry is found */
   1451          				if (ord || sum != sum_sfn(dir))	/* Is there a valid LFN? */
   1452          					dj->lfn_idx = 0xFFFF;		/* It has no LFN. */
   1453          				break;
   1454          			}
   1455          		}
   1456          #else		/* Non LFN configuration */
   1457          		if (c != DDE && (_FS_RPATH || c != '.') && !(dir[DIR_Attr] & AM_VOL))	/* Is it a valid entry? */
   1458          			break;
   1459          #endif
   1460          		res = dir_next(dj, 0);				/* Next entry */
   1461          		if (res != FR_OK) break;
   1462          	}
   1463          
   1464          	if (res != FR_OK) dj->sect = 0;
   1465          
   1466          	return res;
   1467          }
   1468          #endif
