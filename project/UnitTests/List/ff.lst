   3799          /* Divide Physical Drive                                                 */
   3800          /*-----------------------------------------------------------------------*/
   3801          
   3802          FRESULT f_fdisk (
   3803          	BYTE pdrv,			/* Physical drive number */
   3804          	const DWORD szt[],	/* Pointer to the size table for each partitions */
   3805          	void* work			/* Pointer to the working buffer */
   3806          )
   3807          {
   3808          	UINT i, n, sz_cyl, tot_cyl, b_cyl, e_cyl, p_cyl;
   3809          	BYTE s_hd, e_hd, *p, *buf = (BYTE*)work;
   3810          	DSTATUS stat;
   3811          	DWORD sz_disk, sz_part, s_part;
   3812          
   3813          
   3814          	stat = disk_initialize(pdrv);
   3815          	if (stat & STA_NOINIT) return FR_NOT_READY;
   3816          	if (stat & STA_PROTECT) return FR_WRITE_PROTECTED;
   3817          	if (disk_ioctl(pdrv, GET_SECTOR_COUNT, &sz_disk)) return FR_DISK_ERR;
   3818          
   3819          	/* Determine CHS in the table regardless of the drive geometry */
   3820          	for (n = 16; n < 256 && sz_disk / n / 63 > 1024; n *= 2) ;
   3821          	if (n == 256) n--;
   3822          	e_hd = n - 1;
   3823          	sz_cyl = 63 * n;
   3824          	tot_cyl = sz_disk / sz_cyl;
   3825          
   3826          	/* Create partition table */
   3827          	mem_set(buf, 0, _MAX_SS);
   3828          	p = buf + MBR_Table; b_cyl = 0;
   3829          	for (i = 0; i < 4; i++, p += SZ_PTE) {
   3830          		p_cyl = (szt[i] <= 100) ? (DWORD)tot_cyl * szt[i] / 100 : szt[i] / sz_cyl;
   3831          		if (!p_cyl) continue;
   3832          		s_part = (DWORD)sz_cyl * b_cyl;
   3833          		sz_part = (DWORD)sz_cyl * p_cyl;
   3834          		if (i == 0) {	/* Exclude first track of cylinder 0 */
   3835          			s_hd = 1;
   3836          			s_part += 63; sz_part -= 63;
   3837          		} else {
   3838          			s_hd = 0;
   3839          		}
   3840          		e_cyl = b_cyl + p_cyl - 1;
   3841          		if (e_cyl >= tot_cyl) return FR_INVALID_PARAMETER;
   3842          
   3843          		/* Set partition table */
   3844          		p[1] = s_hd;						/* Start head */
   3845          		p[2] = (BYTE)((b_cyl >> 2) + 1);	/* Start sector */
   3846          		p[3] = (BYTE)b_cyl;					/* Start cylinder */
   3847          		p[4] = 0x06;						/* System type (temporary setting) */
   3848          		p[5] = e_hd;						/* End head */
   3849          		p[6] = (BYTE)((e_cyl >> 2) + 63);	/* End sector */
   3850          		p[7] = (BYTE)e_cyl;					/* End cylinder */
   3851          		ST_DWORD(p + 8, s_part);			/* Start sector in LBA */
   3852          		ST_DWORD(p + 12, sz_part);			/* Partition size */
   3853          
   3854          		/* Next partition */
   3855          		b_cyl += p_cyl;
   3856          	}
   3857          	ST_WORD(p, 0xAA55);
   3858          
   3859          	/* Write it to the MBR */
   3860          	return (disk_write(pdrv, buf, 0, 1) || disk_ioctl(pdrv, CTRL_SYNC, 0)) ? FR_DISK_ERR : FR_OK;
   3861          }
