   2646          /*-----------------------------------------------------------------------*/
   2647          /* Close File                                                            */
   2648          /*-----------------------------------------------------------------------*/
   2649          
   2650          FRESULT f_close (
   2651          	FIL *fp		/* Pointer to the file object to be closed */
   2652          )
   2653          {
   2654          	FRESULT res;
   2655          
   2656          #if _FS_READONLY
   2657          	FATFS *fs = fp->fs;
   2658          	res = validate(fs, fp->id);
   2659          	if (res == FR_OK) fp->fs = 0;	/* Discard file object */
   2660          	LEAVE_FF(fs, res);
   2661          
   2662          #else
   2663          	res = f_sync(fp);		/* Flush cached data */
   2664          #if _FS_SHARE
   2665          	if (res == FR_OK) {		/* Decrement open counter */
   2666          #if _FS_REENTRANT
   2667          		res = validate(fp->fs, fp->id);
   2668          		if (res == FR_OK) {
   2669          			res = dec_lock(fp->lockid);
   2670          			unlock_fs(fp->fs, FR_OK);
   2671          		}
   2672          #else
   2673          		res = dec_lock(fp->lockid);
   2674          #endif
   2675          	}
   2676          #endif
   2677          	if (res == FR_OK) fp->fs = 0;	/* Discard file object */
   2678          	return res;
   2679          #endif
   2680          }
