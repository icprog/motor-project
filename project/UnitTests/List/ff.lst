   2810          /*-----------------------------------------------------------------------*/
   2811          /* Seek File R/W Pointer                                                 */
   2812          /*-----------------------------------------------------------------------*/
   2813          
   2814          FRESULT f_lseek (
   2815          	FIL *fp,		/* Pointer to the file object */
   2816          	DWORD ofs		/* File pointer from top of file */
   2817          )
   2818          {
   2819          	FRESULT res;
   2820          
   2821          
   2822          	res = validate(fp->fs, fp->id);		/* Check validity of the object */
   2823          	if (res != FR_OK) LEAVE_FF(fp->fs, res);
   2824          	if (fp->flag & FA__ERROR)			/* Check abort flag */
   2825          		LEAVE_FF(fp->fs, FR_INT_ERR);
   2826          
   2827          #if _USE_FASTSEEK
   2828          	if (fp->cltbl) {	/* Fast seek */
   2829          		DWORD cl, pcl, ncl, tcl, dsc, tlen, ulen, *tbl;
   2830          
   2831          		if (ofs == CREATE_LINKMAP) {	/* Create CLMT */
   2832          			tbl = fp->cltbl;
   2833          			tlen = *tbl++; ulen = 2;	/* Given table size and required table size */
   2834          			cl = fp->sclust;			/* Top of the chain */
   2835          			if (cl) {
   2836          				do {
   2837          					/* Get a fragment */
   2838          					tcl = cl; ncl = 0; ulen += 2;	/* Top, length and used items */
   2839          					do {
   2840          						pcl = cl; ncl++;
   2841          						cl = get_fat(fp->fs, cl);
   2842          						if (cl <= 1) ABORT(fp->fs, FR_INT_ERR);
   2843          						if (cl == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   2844          					} while (cl == pcl + 1);
   2845          					if (ulen <= tlen) {		/* Store the length and top of the fragment */
   2846          						*tbl++ = ncl; *tbl++ = tcl;
   2847          					}
   2848          				} while (cl < fp->fs->n_fatent);	/* Repeat until end of chain */
   2849          			}
   2850          			*fp->cltbl = ulen;	/* Number of items used */
   2851          			if (ulen <= tlen)
   2852          				*tbl = 0;		/* Terminate table */
   2853          			else
   2854          				res = FR_NOT_ENOUGH_CORE;	/* Given table size is smaller than required */
   2855          
   2856          		} else {						/* Fast seek */
   2857          			if (ofs > fp->fsize)		/* Clip offset at the file size */
   2858          				ofs = fp->fsize;
   2859          			fp->fptr = ofs;				/* Set file pointer */
   2860          			if (ofs) {
   2861          				fp->clust = clmt_clust(fp, ofs - 1);
   2862          				dsc = clust2sect(fp->fs, fp->clust);
   2863          				if (!dsc) ABORT(fp->fs, FR_INT_ERR);
   2864          				dsc += (ofs - 1) / SS(fp->fs) & (fp->fs->csize - 1);
   2865          				if (fp->fptr % SS(fp->fs) && dsc != fp->dsect) {	/* Refill sector cache if needed */
   2866          #if !_FS_TINY
   2867          #if !_FS_READONLY
   2868          					if (fp->flag & FA__DIRTY) {		/* Write-back dirty sector cache */
   2869          						if (disk_write(fp->fs->drv, fp->buf, fp->dsect, 1) != RES_OK)
   2870          							ABORT(fp->fs, FR_DISK_ERR);
   2871          						fp->flag &= ~FA__DIRTY;
   2872          					}
   2873          #endif
   2874          					if (disk_read(fp->fs->drv, fp->buf, dsc, 1) != RES_OK)	/* Load current sector */
   2875          						ABORT(fp->fs, FR_DISK_ERR);
   2876          #endif
   2877          					fp->dsect = dsc;
   2878          				}
   2879          			}
   2880          		}
   2881          	} else
   2882          #endif
   2883          
   2884          	/* Normal Seek */
   2885          	{
   2886          		DWORD clst, bcs, nsect, ifptr;
   2887          
   2888          		if (ofs > fp->fsize					/* In read-only mode, clip offset with the file size */
   2889          #if !_FS_READONLY
   2890          			 && !(fp->flag & FA_WRITE)
   2891          #endif
   2892          			) ofs = fp->fsize;
   2893          
   2894          		ifptr = fp->fptr;
   2895          		fp->fptr = nsect = 0;
   2896          		if (ofs) {
   2897          			bcs = (DWORD)fp->fs->csize * SS(fp->fs);	/* Cluster size (byte) */
   2898          			if (ifptr > 0 &&
   2899          				(ofs - 1) / bcs >= (ifptr - 1) / bcs) {	/* When seek to same or following cluster, */
   2900          				fp->fptr = (ifptr - 1) & ~(bcs - 1);	/* start from the current cluster */
   2901          				ofs -= fp->fptr;
   2902          				clst = fp->clust;
   2903          			} else {									/* When seek to back cluster, */
   2904          				clst = fp->sclust;						/* start from the first cluster */
   2905          #if !_FS_READONLY
   2906          				if (clst == 0) {						/* If no cluster chain, create a new chain */
   2907          					clst = create_chain(fp->fs, 0);
   2908          					if (clst == 1) ABORT(fp->fs, FR_INT_ERR);
   2909          					if (clst == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   2910          					fp->sclust = clst;
   2911          				}
   2912          #endif
   2913          				fp->clust = clst;
   2914          			}
   2915          			if (clst != 0) {
   2916          				while (ofs > bcs) {						/* Cluster following loop */
   2917          #if !_FS_READONLY
   2918          					if (fp->flag & FA_WRITE) {			/* Check if in write mode or not */
   2919          						clst = create_chain(fp->fs, clst);	/* Force stretch if in write mode */
   2920          						if (clst == 0) {				/* When disk gets full, clip file size */
   2921          							ofs = bcs; break;
   2922          						}
   2923          					} else
   2924          #endif
   2925          						clst = get_fat(fp->fs, clst);	/* Follow cluster chain if not in write mode */
   2926          					if (clst == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   2927          					if (clst <= 1 || clst >= fp->fs->n_fatent) ABORT(fp->fs, FR_INT_ERR);
   2928          					fp->clust = clst;
   2929          					fp->fptr += bcs;
   2930          					ofs -= bcs;
   2931          				}
   2932          				fp->fptr += ofs;
   2933          				if (ofs % SS(fp->fs)) {
   2934          					nsect = clust2sect(fp->fs, clst);	/* Current sector */
   2935          					if (!nsect) ABORT(fp->fs, FR_INT_ERR);
   2936          					nsect += ofs / SS(fp->fs);
   2937          				}
   2938          			}
   2939          		}
   2940          		if (fp->fptr % SS(fp->fs) && nsect != fp->dsect) {	/* Fill sector cache if needed */
   2941          #if !_FS_TINY
   2942          #if !_FS_READONLY
   2943          			if (fp->flag & FA__DIRTY) {			/* Write-back dirty sector cache */
   2944          				if (disk_write(fp->fs->drv, fp->buf, fp->dsect, 1) != RES_OK)
   2945          					ABORT(fp->fs, FR_DISK_ERR);
   2946          				fp->flag &= ~FA__DIRTY;
   2947          			}
   2948          #endif
   2949          			if (disk_read(fp->fs->drv, fp->buf, nsect, 1) != RES_OK)	/* Fill sector cache */
   2950          				ABORT(fp->fs, FR_DISK_ERR);
   2951          #endif
   2952          			fp->dsect = nsect;
   2953          		}
   2954          #if !_FS_READONLY
   2955          		if (fp->fptr > fp->fsize) {			/* Set file change flag if the file size is extended */
   2956          			fp->fsize = fp->fptr;
   2957          			fp->flag |= FA__WRITTEN;
   2958          		}
   2959          #endif
   2960          	}
   2961          
   2962          	LEAVE_FF(fp->fs, res);
   2963          }
