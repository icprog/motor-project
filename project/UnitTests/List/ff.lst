   2210          /* Mount/Unmount a Logical Drive                                         */
   2211          /*-----------------------------------------------------------------------*/
   2212          
   2213          FRESULT f_mount (
   2214          	BYTE vol,		/* Logical drive number to be mounted/unmounted */
   2215          	FATFS *fs		/* Pointer to new file system object (NULL for unmount)*/
   2216          )
   2217          {
   2218          	FATFS *rfs;
   2219          
   2220          
   2221          	if (vol >= _VOLUMES)		/* Check if the drive number is valid */
   2222          		return FR_INVALID_DRIVE;
   2223          	rfs = FatFs[vol];			/* Get current fs object */
   2224          
   2225          	if (rfs) {
   2226          #if _FS_SHARE
   2227          		clear_lock(rfs);
   2228          #endif
   2229          #if _FS_REENTRANT				/* Discard sync object of the current volume */
   2230          		if (!ff_del_syncobj(rfs->sobj)) return FR_INT_ERR;
   2231          #endif
   2232          		rfs->fs_type = 0;		/* Clear old fs object */
   2233          	}
   2234          
   2235          	if (fs) {
   2236          		fs->fs_type = 0;		/* Clear new fs object */
   2237          #if _FS_REENTRANT				/* Create sync object for the new volume */
   2238          		if (!ff_cre_syncobj(vol, &fs->sobj)) return FR_INT_ERR;
   2239          #endif
   2240          	}
   2241          	FatFs[vol] = fs;			/* Register new fs object */
   2242          
   2243          	return FR_OK;
   2244          }
