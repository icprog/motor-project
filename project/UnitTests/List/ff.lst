   3497          
   3498          /*-----------------------------------------------------------------------*/
   3499          /* Forward data to the stream directly (available on only tiny cfg)      */
   3500          /*-----------------------------------------------------------------------*/
   3501          #if _USE_FORWARD && _FS_TINY
   3502          
   3503          FRESULT f_forward (
   3504          	FIL *fp, 						/* Pointer to the file object */
   3505          	UINT (*func)(const BYTE*,UINT),	/* Pointer to the streaming function */
   3506          	UINT btr,						/* Number of bytes to forward */
   3507          	UINT *bf						/* Pointer to number of bytes forwarded */
   3508          )
   3509          {
   3510          	FRESULT res;
   3511          	DWORD remain, clst, sect;
   3512          	UINT rcnt;
   3513          	BYTE csect;
   3514          
   3515          
   3516          	*bf = 0;	/* Initialize byte counter */
   3517          
   3518          	res = validate(fp->fs, fp->id);					/* Check validity of the object */
   3519          	if (res != FR_OK) LEAVE_FF(fp->fs, res);
   3520          	if (fp->flag & FA__ERROR)						/* Check error flag */
   3521          		LEAVE_FF(fp->fs, FR_INT_ERR);
   3522          	if (!(fp->flag & FA_READ))						/* Check access mode */
   3523          		LEAVE_FF(fp->fs, FR_DENIED);
   3524          
   3525          	remain = fp->fsize - fp->fptr;
   3526          	if (btr > remain) btr = (UINT)remain;			/* Truncate btr by remaining bytes */
   3527          
   3528          	for ( ;  btr && (*func)(0, 0);					/* Repeat until all data transferred or stream becomes busy */
   3529          		fp->fptr += rcnt, *bf += rcnt, btr -= rcnt) {
   3530          		csect = (BYTE)(fp->fptr / SS(fp->fs) & (fp->fs->csize - 1));	/* Sector offset in the cluster */
   3531          		if ((fp->fptr % SS(fp->fs)) == 0) {			/* On the sector boundary? */
   3532          			if (!csect) {							/* On the cluster boundary? */
   3533          				clst = (fp->fptr == 0) ?			/* On the top of the file? */
   3534          					fp->sclust : get_fat(fp->fs, fp->clust);
   3535          				if (clst <= 1) ABORT(fp->fs, FR_INT_ERR);
   3536          				if (clst == 0xFFFFFFFF) ABORT(fp->fs, FR_DISK_ERR);
   3537          				fp->clust = clst;					/* Update current cluster */
   3538          			}
   3539          		}
   3540          		sect = clust2sect(fp->fs, fp->clust);		/* Get current data sector */
   3541          		if (!sect) ABORT(fp->fs, FR_INT_ERR);
   3542          		sect += csect;
   3543          		if (move_window(fp->fs, sect))				/* Move sector window */
   3544          			ABORT(fp->fs, FR_DISK_ERR);
   3545          		fp->dsect = sect;
   3546          		rcnt = SS(fp->fs) - (WORD)(fp->fptr % SS(fp->fs));	/* Forward data from sector window */
   3547          		if (rcnt > btr) rcnt = btr;
   3548          		rcnt = (*func)(&fp->fs->win[(WORD)fp->fptr % SS(fp->fs)], rcnt);
   3549          		if (!rcnt) ABORT(fp->fs, FR_INT_ERR);
   3550          	}
   3551          
   3552          	LEAVE_FF(fp->fs, FR_OK);
   3553          }
   3554          #endif /* _USE_FORWARD */
   3555          
   3556          
   3557          
   3558          #if _USE_MKFS && !_FS_READONLY
   3559          /*-----------------------------------------------------------------------*/
   3560          /* Create File System on the Drive                                       */
   3561          /*-----------------------------------------------------------------------*/
   3562          #define N_ROOTDIR	512		/* Number of root dir entries for FAT12/16 */
   3563          #define N_FATS		1		/* Number of FAT copies (1 or 2) */
   3564          
   3565          
   3566          FRESULT f_mkfs (
   3567          	BYTE drv,		/* Logical drive number */
   3568          	BYTE sfd,		/* Partitioning rule 0:FDISK, 1:SFD */
   3569          	UINT au			/* Allocation unit size [bytes] */
   3570          )
   3571          {
   3572          	static const WORD vst[] = { 1024,   512,  256,  128,   64,    32,   16,    8,    4,    2,   0};
   3573          	static const WORD cst[] = {32768, 16384, 8192, 4096, 2048, 16384, 8192, 4096, 2048, 1024, 512};
   3574          	BYTE fmt, md, sys, *tbl, pdrv, part;
   3575          	DWORD n_clst, vs, n, wsect;
   3576          	UINT i;
   3577          	DWORD b_vol, b_fat, b_dir, b_data;	/* LBA */
   3578          	DWORD n_vol, n_rsv, n_fat, n_dir;	/* Size */
   3579          	FATFS *fs;
   3580          	DSTATUS stat;
   3581          
   3582          
   3583          	/* Check mounted drive and clear work area */
   3584          	if (drv >= _VOLUMES) return FR_INVALID_DRIVE;
   3585          	if (sfd > 1) return FR_INVALID_PARAMETER;
   3586          	if (au & (au - 1)) return FR_INVALID_PARAMETER;
   3587          	fs = FatFs[drv];
   3588          	if (!fs) return FR_NOT_ENABLED;
   3589          	fs->fs_type = 0;
   3590          	pdrv = LD2PD(drv);	/* Physical drive */
   3591          	part = LD2PT(drv);	/* Partition (0:auto detect, 1-4:get from partition table)*/
   3592          
   3593          	/* Get disk statics */
   3594          	stat = disk_initialize(pdrv);
   3595          	if (stat & STA_NOINIT) return FR_NOT_READY;
   3596          	if (stat & STA_PROTECT) return FR_WRITE_PROTECTED;
   3597          #if _MAX_SS != 512					/* Get disk sector size */
   3598          	if (disk_ioctl(pdrv, GET_SECTOR_SIZE, &SS(fs)) != RES_OK || SS(fs) > _MAX_SS)
   3599          		return FR_DISK_ERR;
   3600          #endif
   3601          	if (_MULTI_PARTITION && part) {
   3602          		/* Get partition information from partition table in the MBR */
   3603          		if (disk_read(pdrv, fs->win, 0, 1) != RES_OK) return FR_DISK_ERR;
   3604          		if (LD_WORD(fs->win+BS_55AA) != 0xAA55) return FR_MKFS_ABORTED;
   3605          		tbl = &fs->win[MBR_Table + (part - 1) * SZ_PTE];
   3606          		if (!tbl[4]) return FR_MKFS_ABORTED;	/* No partition? */
   3607          		b_vol = LD_DWORD(tbl+8);	/* Volume start sector */
   3608          		n_vol = LD_DWORD(tbl+12);	/* Volume size */
   3609          	} else {
   3610          		/* Create a partition in this function */
   3611          		if (disk_ioctl(pdrv, GET_SECTOR_COUNT, &n_vol) != RES_OK || n_vol < 128)
   3612          			return FR_DISK_ERR;
   3613          		b_vol = (sfd) ? 0 : 63;		/* Volume start sector */
   3614          		n_vol -= b_vol;				/* Volume size */
   3615          	}
   3616          
   3617          	if (!au) {				/* AU auto selection */
   3618          		vs = n_vol / (2000 / (SS(fs) / 512));
   3619          		for (i = 0; vs < vst[i]; i++) ;
   3620          		au = cst[i];
   3621          	}
   3622          	au /= SS(fs);		/* Number of sectors per cluster */
   3623          	if (au == 0) au = 1;
   3624          	if (au > 128) au = 128;
   3625          
   3626          	/* Pre-compute number of clusters and FAT syb-type */
   3627          	n_clst = n_vol / au;
   3628          	fmt = FS_FAT12;
   3629          	if (n_clst >= MIN_FAT16) fmt = FS_FAT16;
   3630          	if (n_clst >= MIN_FAT32) fmt = FS_FAT32;
   3631          
   3632          	/* Determine offset and size of FAT structure */
   3633          	if (fmt == FS_FAT32) {
   3634          		n_fat = ((n_clst * 4) + 8 + SS(fs) - 1) / SS(fs);
   3635          		n_rsv = 32;
   3636          		n_dir = 0;
   3637          	} else {
   3638          		n_fat = (fmt == FS_FAT12) ? (n_clst * 3 + 1) / 2 + 3 : (n_clst * 2) + 4;
   3639          		n_fat = (n_fat + SS(fs) - 1) / SS(fs);
   3640          		n_rsv = 1;
   3641          		n_dir = (DWORD)N_ROOTDIR * SZ_DIR / SS(fs);
   3642          	}
   3643          	b_fat = b_vol + n_rsv;				/* FAT area start sector */
   3644          	b_dir = b_fat + n_fat * N_FATS;		/* Directory area start sector */
   3645          	b_data = b_dir + n_dir;				/* Data area start sector */
   3646          	if (n_vol < b_data + au - b_vol) return FR_MKFS_ABORTED;	/* Too small volume */
   3647          
   3648          	/* Align data start sector to erase block boundary (for flash memory media) */
   3649          	if (disk_ioctl(pdrv, GET_BLOCK_SIZE, &n) != RES_OK || !n || n > 32768) n = 1;
   3650          	n = (b_data + n - 1) & ~(n - 1);	/* Next nearest erase block from current data start */
   3651          	n = (n - b_data) / N_FATS;
   3652          	if (fmt == FS_FAT32) {		/* FAT32: Move FAT offset */
   3653          		n_rsv += n;
   3654          		b_fat += n;
   3655          	} else {					/* FAT12/16: Expand FAT size */
   3656          		n_fat += n;
   3657          	}
   3658          
   3659          	/* Determine number of clusters and final check of validity of the FAT sub-type */
   3660          	n_clst = (n_vol - n_rsv - n_fat * N_FATS - n_dir) / au;
   3661          	if (   (fmt == FS_FAT16 && n_clst < MIN_FAT16)
   3662          		|| (fmt == FS_FAT32 && n_clst < MIN_FAT32))
   3663          		return FR_MKFS_ABORTED;
   3664          
   3665          	switch (fmt) {	/* Determine system ID for partition table */
   3666          	case FS_FAT12:	sys = 0x01; break;
   3667          	case FS_FAT16:	sys = (n_vol < 0x10000) ? 0x04 : 0x06; break;
   3668          	default: 		sys = 0x0C;
   3669          	}
   3670          
   3671          	if (_MULTI_PARTITION && part) {
   3672          		/* Update system ID in the partition table */
   3673          		tbl = &fs->win[MBR_Table + (part - 1) * SZ_PTE];
   3674          		tbl[4] = sys;
   3675          		if (disk_write(pdrv, fs->win, 0, 1) != RES_OK) return FR_DISK_ERR;
   3676          		md = 0xF8;
   3677          	} else {
   3678          		if (sfd) {	/* No patition table (SFD) */
   3679          			md = 0xF0;
   3680          		} else {	/* Create partition table (FDISK) */
   3681          			mem_set(fs->win, 0, SS(fs));
   3682          			tbl = fs->win+MBR_Table;	/* Create partiton table for single partition in the drive */
   3683          			tbl[1] = 1;						/* Partition start head */
   3684          			tbl[2] = 1;						/* Partition start sector */
   3685          			tbl[3] = 0;						/* Partition start cylinder */
   3686          			tbl[4] = sys;					/* System type */
   3687          			tbl[5] = 254;					/* Partition end head */
   3688          			n = (b_vol + n_vol) / 63 / 255;
   3689          			tbl[6] = (BYTE)((n >> 2) | 63);	/* Partiiton end sector */
   3690          			tbl[7] = (BYTE)n;				/* End cylinder */
   3691          			ST_DWORD(tbl+8, 63);			/* Partition start in LBA */
   3692          			ST_DWORD(tbl+12, n_vol);		/* Partition size in LBA */
   3693          			ST_WORD(fs->win+BS_55AA, 0xAA55);	/* MBR signature */
   3694          			if (disk_write(pdrv, fs->win, 0, 1) != RES_OK)	/* Write it to the MBR sector */
   3695          				return FR_DISK_ERR;
   3696          			md = 0xF8;
   3697          		}
   3698          	}
   3699          
   3700          	/* Create BPB in the VBR */
   3701          	tbl = fs->win;							/* Clear sector */
   3702          	mem_set(tbl, 0, SS(fs));
