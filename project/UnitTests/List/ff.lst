   2704          FRESULT f_chdir (
   2705          	const TCHAR *path	/* Pointer to the directory path */
   2706          )
   2707          {
   2708          	FRESULT res;
   2709          	DIR dj;
   2710          	DEF_NAMEBUF;
   2711          
   2712          
   2713          	res = chk_mounted(&path, &dj.fs, 0);
   2714          	if (res == FR_OK) {
   2715          		INIT_BUF(dj);
   2716          		res = follow_path(&dj, path);		/* Follow the path */
   2717          		FREE_BUF();
   2718          		if (res == FR_OK) {					/* Follow completed */
   2719          			if (!dj.dir) {
   2720          				dj.fs->cdir = dj.sclust;	/* Start directory itself */
   2721          			} else {
   2722          				if (dj.dir[DIR_Attr] & AM_DIR)	/* Reached to the directory */
   2723          					dj.fs->cdir = LD_CLUST(dj.dir);
   2724          				else
   2725          					res = FR_NO_PATH;		/* Reached but a file */
   2726          			}
   2727          		}
   2728          		if (res == FR_NO_FILE) res = FR_NO_PATH;
   2729          	}
   2730          
   2731          	LEAVE_FF(dj.fs, res);
   2732          }
