   3733          		tbl[BS_DrvNum] = 0x80;				/* Drive number */
   3734          		tbl[BS_BootSig] = 0x29;				/* Extended boot signature */
   3735          		mem_cpy(tbl+BS_VolLab, "NO NAME    " "FAT     ", 19);	/* Volume label, FAT signature */
