   2972          FRESULT f_opendir (
   2973          	DIR *dj,			/* Pointer to directory object to create */
   2974          	const TCHAR *path	/* Pointer to the directory path */
   2975          )
   2976          {
   2977          	FRESULT res;
   2978          	DEF_NAMEBUF;
