   2250          /* Open or Create a File                                                 */
   2251          /*-----------------------------------------------------------------------*/
   2252          
   2253          FRESULT f_open (
   2254          	FIL *fp,			/* Pointer to the blank file object */
   2255          	const TCHAR *path,	/* Pointer to the file name */
   2256          	BYTE mode			/* Access mode and file open mode flags */
   2257          )
   2258          {
   2259          	FRESULT res;
   2260          	DIR dj;
   2261          	BYTE *dir;
   2262          	DEF_NAMEBUF;
   2263          
   2264          
   2265          	fp->fs = 0;			/* Clear file object */
   2266          
   2267          #if !_FS_READONLY
   2268          	mode &= FA_READ | FA_WRITE | FA_CREATE_ALWAYS | FA_OPEN_ALWAYS | FA_CREATE_NEW;
   2269          	res = chk_mounted(&path, &dj.fs, (BYTE)(mode & ~FA_READ));
   2270          #else
   2271          	mode &= FA_READ;
   2272          	res = chk_mounted(&path, &dj.fs, 0);
   2273          #endif
   2274          	INIT_BUF(dj);
   2275          	if (res == FR_OK)
   2276          		res = follow_path(&dj, path);	/* Follow the file path */
   2277          	dir = dj.dir;
   2278          
   2279          #if !_FS_READONLY	/* R/W configuration */
   2280          	if (res == FR_OK) {
   2281          		if (!dir)	/* Current dir itself */
   2282          			res = FR_INVALID_NAME;
   2283          #if _FS_SHARE
   2284          		else
   2285          			res = chk_lock(&dj, (mode & ~FA_READ) ? 1 : 0);
   2286          #endif
   2287          	}
   2288          	/* Create or Open a file */
   2289          	if (mode & (FA_CREATE_ALWAYS | FA_OPEN_ALWAYS | FA_CREATE_NEW)) {
   2290          		DWORD dw, cl;
   2291          
   2292          		if (res != FR_OK) {					/* No file, create new */
   2293          			if (res == FR_NO_FILE)			/* There is no file to open, create a new entry */
   2294          #if _FS_SHARE
   2295          				res = enq_lock() ? dir_register(&dj) : FR_TOO_MANY_OPEN_FILES;
   2296          #else
   2297          				res = dir_register(&dj);
   2298          #endif
   2299          			mode |= FA_CREATE_ALWAYS;		/* File is created */
   2300          			dir = dj.dir;					/* New entry */
   2301          		}
   2302          		else {								/* Any object is already existing */
   2303          			if (dir[DIR_Attr] & (AM_RDO | AM_DIR)) {	/* Cannot overwrite it (R/O or DIR) */
   2304          				res = FR_DENIED;
   2305          			} else {
   2306          				if (mode & FA_CREATE_NEW)	/* Cannot create as new file */
   2307          					res = FR_EXIST;
   2308          			}
   2309          		}
   2310          		if (res == FR_OK && (mode & FA_CREATE_ALWAYS)) {	/* Truncate it if overwrite mode */
   2311          			dw = get_fattime();					/* Created time */
   2312          			ST_DWORD(dir+DIR_CrtTime, dw);
   2313          			dir[DIR_Attr] = 0;					/* Reset attribute */
   2314          			ST_DWORD(dir+DIR_FileSize, 0);		/* size = 0 */
   2315          			cl = LD_CLUST(dir);					/* Get start cluster */
   2316          			ST_CLUST(dir, 0);					/* cluster = 0 */
   2317          			dj.fs->wflag = 1;
   2318          			if (cl) {							/* Remove the cluster chain if exist */
   2319          				dw = dj.fs->winsect;
   2320          				res = remove_chain(dj.fs, cl);
   2321          				if (res == FR_OK) {
   2322          					dj.fs->last_clust = cl - 1;	/* Reuse the cluster hole */
   2323          					res = move_window(dj.fs, dw);
   2324          				}
   2325          			}
   2326          		}
   2327          	}
   2328          	else {	/* Open an existing file */
   2329          		if (res == FR_OK) {						/* Follow succeeded */
   2330          			if (dir[DIR_Attr] & AM_DIR) {		/* It is a directory */
   2331          				res = FR_NO_FILE;
   2332          			} else {
   2333          				if ((mode & FA_WRITE) && (dir[DIR_Attr] & AM_RDO)) /* R/O violation */
   2334          					res = FR_DENIED;
   2335          			}
   2336          		}
   2337          	}
   2338          	if (res == FR_OK) {
   2339          		if (mode & FA_CREATE_ALWAYS)			/* Set file change flag if created or overwritten */
   2340          			mode |= FA__WRITTEN;
   2341          		fp->dir_sect = dj.fs->winsect;			/* Pointer to the directory entry */
   2342          		fp->dir_ptr = dir;
   2343          #if _FS_SHARE
   2344          		fp->lockid = inc_lock(&dj, (mode & ~FA_READ) ? 1 : 0);
   2345          		if (!fp->lockid) res = FR_INT_ERR;
   2346          #endif
   2347          	}
   2348          
   2349          #else				/* R/O configuration */
   2350          	if (res == FR_OK) {					/* Follow succeeded */
   2351          		if (!dir) {						/* Current dir itself */
   2352          			res = FR_INVALID_NAME;
   2353          		} else {
   2354          			if (dir[DIR_Attr] & AM_DIR)	/* It is a directory */
   2355          				res = FR_NO_FILE;
   2356          		}
   2357          	}
   2358          #endif
   2359          	FREE_BUF();
   2360          
   2361          	if (res == FR_OK) {
   2362          		fp->flag = mode;					/* File access mode */
   2363          		fp->sclust = LD_CLUST(dir);			/* File start cluster */
   2364          		fp->fsize = LD_DWORD(dir+DIR_FileSize);	/* File size */
   2365          		fp->fptr = 0;						/* File pointer */
   2366          		fp->dsect = 0;
   2367          #if _USE_FASTSEEK
   2368          		fp->cltbl = 0;						/* Normal seek mode */
   2369          #endif
   2370          		fp->fs = dj.fs; fp->id = dj.fs->id;	/* Validate file object */
   2371          	}
   2372          
   2373          	LEAVE_FF(dj.fs, res);
   2374          }
   2375          
   2376          
