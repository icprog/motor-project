   3420          /*-----------------------------------------------------------------------*/
   3421          /* Rename File/Directory                                                 */
   3422          /*-----------------------------------------------------------------------*/
   3423          
   3424          FRESULT f_rename (
   3425          	const TCHAR *path_old,	/* Pointer to the old name */
   3426          	const TCHAR *path_new	/* Pointer to the new name */
   3427          )
   3428          {
   3429          	FRESULT res;
   3430          	DIR djo, djn;
   3431          	BYTE buf[21], *dir;
   3432          	DWORD dw;
   3433          	DEF_NAMEBUF;
   3434          
   3435          
   3436          	res = chk_mounted(&path_old, &djo.fs, 1);
   3437          	if (res == FR_OK) {
   3438          		djn.fs = djo.fs;
   3439          		INIT_BUF(djo);
   3440          		res = follow_path(&djo, path_old);		/* Check old object */
   3441          		if (_FS_RPATH && res == FR_OK && (djo.fn[NS] & NS_DOT))
   3442          			res = FR_INVALID_NAME;
   3443          #if _FS_SHARE
   3444          		if (res == FR_OK) res = chk_lock(&djo, 2);
   3445          #endif
   3446          		if (res == FR_OK) {						/* Old object is found */
   3447          			if (!djo.dir) {						/* Is root dir? */
   3448          				res = FR_NO_FILE;
   3449          			} else {
   3450          				mem_cpy(buf, djo.dir+DIR_Attr, 21);		/* Save the object information except for name */
   3451          				mem_cpy(&djn, &djo, sizeof(DIR));		/* Check new object */
   3452          				res = follow_path(&djn, path_new);
   3453          				if (res == FR_OK) res = FR_EXIST;		/* The new object name is already existing */
   3454          				if (res == FR_NO_FILE) { 				/* Is it a valid path and no name collision? */
   3455          /* Start critical section that any interruption or error can cause cross-link */
   3456          					res = dir_register(&djn);			/* Register the new entry */
   3457          					if (res == FR_OK) {
   3458          						dir = djn.dir;					/* Copy object information except for name */
   3459          						mem_cpy(dir+13, buf+2, 19);
   3460          						dir[DIR_Attr] = buf[0] | AM_ARC;
   3461          						djo.fs->wflag = 1;
   3462          						if (djo.sclust != djn.sclust && (dir[DIR_Attr] & AM_DIR)) {		/* Update .. entry in the directory if needed */
   3463          							dw = clust2sect(djn.fs, LD_CLUST(dir));
   3464          							if (!dw) {
   3465          								res = FR_INT_ERR;
   3466          							} else {
   3467          								res = move_window(djn.fs, dw);
   3468          								dir = djn.fs->win+SZ_DIR;	/* .. entry */
   3469          								if (res == FR_OK && dir[1] == '.') {
   3470          									dw = (djn.fs->fs_type == FS_FAT32 && djn.sclust == djn.fs->dirbase) ? 0 : djn.sclust;
   3471          									ST_CLUST(dir, dw);
   3472          									djn.fs->wflag = 1;
   3473          								}
   3474          							}
   3475          						}
   3476          						if (res == FR_OK) {
   3477          							res = dir_remove(&djo);		/* Remove old entry */
   3478          							if (res == FR_OK)
   3479          								res = sync(djo.fs);
   3480          						}
   3481          					}
   3482          /* End critical section */
   3483          				}
   3484          			}
   3485          		}
   3486          		FREE_BUF();
   3487          	}
   3488          	LEAVE_FF(djo.fs, res);
   3489          }
