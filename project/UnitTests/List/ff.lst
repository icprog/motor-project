   1471          
   1472          /*-----------------------------------------------------------------------*/
   1473          /* Register an object to the directory                                   */
   1474          /*-----------------------------------------------------------------------*/
   1475          #if !_FS_READONLY
   1476          static
   1477          FRESULT dir_register (	/* FR_OK:Successful, FR_DENIED:No free entry or too many SFN collision, FR_DISK_ERR:Disk error */
   1478          	DIR *dj				/* Target directory with object name to be created */
   1479          )
   1480          {
   1481          	FRESULT res;
   1482          	BYTE c, *dir;
   1483          #if _USE_LFN	/* LFN configuration */
   1484          	WORD n, ne, is;
   1485          	BYTE sn[12], *fn, sum;
   1486          	WCHAR *lfn;
   1487          
   1488          
   1489          	fn = dj->fn; lfn = dj->lfn;
   1490          	mem_cpy(sn, fn, 12);
   1491          
   1492          	if (_FS_RPATH && (sn[NS] & NS_DOT))		/* Cannot create dot entry */
   1493          		return FR_INVALID_NAME;
   1494          
   1495          	if (sn[NS] & NS_LOSS) {			/* When LFN is out of 8.3 format, generate a numbered name */
   1496          		fn[NS] = 0; dj->lfn = 0;			/* Find only SFN */
   1497          		for (n = 1; n < 100; n++) {
   1498          			gen_numname(fn, sn, lfn, n);	/* Generate a numbered name */
   1499          			res = dir_find(dj);				/* Check if the name collides with existing SFN */
   1500          			if (res != FR_OK) break;
   1501          		}
   1502          		if (n == 100) return FR_DENIED;		/* Abort if too many collisions */
   1503          		if (res != FR_NO_FILE) return res;	/* Abort if the result is other than 'not collided' */
   1504          		fn[NS] = sn[NS]; dj->lfn = lfn;
   1505          	}
   1506          
   1507          	if (sn[NS] & NS_LFN) {			/* When LFN is to be created, reserve an SFN + LFN entries. */
   1508          		for (ne = 0; lfn[ne]; ne++) ;
   1509          		ne = (ne + 25) / 13;
   1510          	} else {						/* Otherwise reserve only an SFN entry. */
   1511          		ne = 1;
   1512          	}
   1513          
   1514          	/* Reserve contiguous entries */
   1515          	res = dir_sdi(dj, 0);
   1516          	if (res != FR_OK) return res;
   1517          	n = is = 0;
   1518          	do {
   1519          		res = move_window(dj->fs, dj->sect);
   1520          		if (res != FR_OK) break;
   1521          		c = *dj->dir;				/* Check the entry status */
   1522          		if (c == DDE || c == 0) {	/* Is it a blank entry? */
   1523          			if (n == 0) is = dj->index;	/* First index of the contiguous entry */
   1524          			if (++n == ne) break;	/* A contiguous entry that required count is found */
   1525          		} else {
   1526          			n = 0;					/* Not a blank entry. Restart to search */
   1527          		}
   1528          		res = dir_next(dj, 1);		/* Next entry with table stretch */
   1529          	} while (res == FR_OK);
   1530          
   1531          	if (res == FR_OK && ne > 1) {	/* Initialize LFN entry if needed */
   1532          		res = dir_sdi(dj, is);
   1533          		if (res == FR_OK) {
   1534          			sum = sum_sfn(dj->fn);	/* Sum of the SFN tied to the LFN */
   1535          			ne--;
   1536          			do {					/* Store LFN entries in bottom first */
   1537          				res = move_window(dj->fs, dj->sect);
   1538          				if (res != FR_OK) break;
   1539          				fit_lfn(dj->lfn, dj->dir, (BYTE)ne, sum);
   1540          				dj->fs->wflag = 1;
   1541          				res = dir_next(dj, 0);	/* Next entry */
   1542          			} while (res == FR_OK && --ne);
   1543          		}
   1544          	}
   1545          
   1546          #else	/* Non LFN configuration */
   1547          	res = dir_sdi(dj, 0);
   1548          	if (res == FR_OK) {
   1549          		do {	/* Find a blank entry for the SFN */
   1550          			res = move_window(dj->fs, dj->sect);
   1551          			if (res != FR_OK) break;
   1552          			c = *dj->dir;
   1553          			if (c == DDE || c == 0) break;	/* Is it a blank entry? */
   1554          			res = dir_next(dj, 1);			/* Next entry with table stretch */
   1555          		} while (res == FR_OK);
   1556          	}
   1557          #endif
   1558          
   1559          	if (res == FR_OK) {		/* Initialize the SFN entry */
   1560          		res = move_window(dj->fs, dj->sect);
   1561          		if (res == FR_OK) {
   1562          			dir = dj->dir;
   1563          			mem_set(dir, 0, SZ_DIR);	/* Clean the entry */
   1564          			mem_cpy(dir, dj->fn, 11);	/* Put SFN */
   1565          #if _USE_LFN
   1566          			dir[DIR_NTres] = *(dj->fn+NS) & (NS_BODY | NS_EXT);	/* Put NT flag */
   1567          #endif
   1568          			dj->fs->wflag = 1;
   1569          		}
   1570          	}
   1571          
   1572          	return res;
   1573          }
   1574          #endif /* !_FS_READONLY */
   1575          
   1576          
   1577          
   1578          
   1579          /*-----------------------------------------------------------------------*/
   1580          /* Remove an object from the directory                                   */
   1581          /*-----------------------------------------------------------------------*/
   1582          #if !_FS_READONLY && !_FS_MINIMIZE
   1583          static
   1584          FRESULT dir_remove (	/* FR_OK: Successful, FR_DISK_ERR: A disk error */
   1585          	DIR *dj				/* Directory object pointing the entry to be removed */
   1586          )
   1587          {
   1588          	FRESULT res;
   1589          #if _USE_LFN	/* LFN configuration */
   1590          	WORD i;
   1591          
   1592          	i = dj->index;	/* SFN index */
   1593          	res = dir_sdi(dj, (WORD)((dj->lfn_idx == 0xFFFF) ? i : dj->lfn_idx));	/* Goto the SFN or top of the LFN entries */
   1594          	if (res == FR_OK) {
   1595          		do {
   1596          			res = move_window(dj->fs, dj->sect);
   1597          			if (res != FR_OK) break;
   1598          			*dj->dir = DDE;			/* Mark the entry "deleted" */
   1599          			dj->fs->wflag = 1;
   1600          			if (dj->index >= i) break;	/* When reached SFN, all entries of the object has been deleted. */
   1601          			res = dir_next(dj, 0);		/* Next entry */
   1602          		} while (res == FR_OK);
   1603          		if (res == FR_NO_FILE) res = FR_INT_ERR;
   1604          	}
   1605          
   1606          #else			/* Non LFN configuration */
   1607          	res = dir_sdi(dj, dj->index);
   1608          	if (res == FR_OK) {
   1609          		res = move_window(dj->fs, dj->sect);
   1610          		if (res == FR_OK) {
   1611          			*dj->dir = DDE;			/* Mark the entry "deleted" */
   1612          			dj->fs->wflag = 1;
   1613          		}
   1614          	}
   1615          #endif
   1616          
   1617          	return res;
   1618          }
   1619          #endif /* !_FS_READONLY */
   1620          
   1621          
   1622          
   1623          
   1624          /*-----------------------------------------------------------------------*/
   1625          /* Pick a segment and create the object name in directory form           */
   1626          /*-----------------------------------------------------------------------*/
   1627          
   1628          static
   1629          FRESULT create_name (
   1630          	DIR *dj,			/* Pointer to the directory object */
   1631          	const TCHAR **path	/* Pointer to pointer to the segment in the path string */
   1632          )
   1633          {
   1634          #ifdef _EXCVT
   1635          	static const BYTE excvt[] = _EXCVT;	/* Upper conversion table for extended chars */
   1636          #endif
   1637          
   1638          #if _USE_LFN	/* LFN configuration */
   1639          	BYTE b, cf;
   1640          	WCHAR w, *lfn;
   1641          	UINT i, ni, si, di;
   1642          	const TCHAR *p;
   1643          
   1644          	/* Create LFN in Unicode */
   1645          	for (p = *path; *p == '/' || *p == '\\'; p++) ;	/* Strip duplicated separator */
   1646          	lfn = dj->lfn;
   1647          	si = di = 0;
   1648          	for (;;) {
   1649          		w = p[si++];					/* Get a character */
   1650          		if (w < ' ' || w == '/' || w == '\\') break;	/* Break on end of segment */
   1651          		if (di >= _MAX_LFN)				/* Reject too long name */
   1652          			return FR_INVALID_NAME;
   1653          #if !_LFN_UNICODE
   1654          		w &= 0xFF;
   1655          		if (IsDBCS1(w)) {				/* Check if it is a DBC 1st byte (always false on SBCS cfg) */
   1656          			b = (BYTE)p[si++];			/* Get 2nd byte */
   1657          #if _DF1S
   1658          			if (!IsDBCS2(b))
   1659          				return FR_INVALID_NAME;	/* Reject invalid sequence */
   1660          			w = (w << 8) + b;			/* Create a DBC */
   1661          #else
   1662          			return FR_INVALID_NAME;		/* Reject invalid sequence */
   1663          #endif
   1664          		}
   1665          		w = ff_convert(w, 1);			/* Convert ANSI/OEM to Unicode */
   1666          		if (!w) return FR_INVALID_NAME;	/* Reject invalid code */
   1667          #endif
   1668          		if (w < 0x80 && chk_chr("\"*:<>\?|\x7F", w)) /* Reject illegal chars for LFN */
   1669          			return FR_INVALID_NAME;
   1670          		lfn[di++] = w;					/* Store the Unicode char */
   1671          	}
   1672          	*path = &p[si];						/* Return pointer to the next segment */
   1673          	cf = (w < ' ') ? NS_LAST : 0;		/* Set last segment flag if end of path */
   1674          #if _FS_RPATH
   1675          	if ((di == 1 && lfn[di-1] == '.') || /* Is this a dot entry? */
   1676          		(di == 2 && lfn[di-1] == '.' && lfn[di-2] == '.')) {
   1677          		lfn[di] = 0;
   1678          		for (i = 0; i < 11; i++)
   1679          			dj->fn[i] = (i < di) ? '.' : ' ';
   1680          		dj->fn[i] = cf | NS_DOT;		/* This is a dot entry */
   1681          		return FR_OK;
   1682          	}
   1683          #endif
   1684          	while (di) {						/* Strip trailing spaces and dots */
   1685          		w = lfn[di-1];
   1686          		if (w != ' ' && w != '.') break;
   1687          		di--;
   1688          	}
   1689          	if (!di) return FR_INVALID_NAME;	/* Reject nul string */
   1690          
   1691          	lfn[di] = 0;						/* LFN is created */
   1692          
   1693          	/* Create SFN in directory form */
   1694          	mem_set(dj->fn, ' ', 11);
   1695          	for (si = 0; lfn[si] == ' ' || lfn[si] == '.'; si++) ;	/* Strip leading spaces and dots */
   1696          	if (si) cf |= NS_LOSS | NS_LFN;
   1697          	while (di && lfn[di - 1] != '.') di--;	/* Find extension (di<=si: no extension) */
   1698          
   1699          	b = i = 0; ni = 8;
   1700          	for (;;) {
   1701          		w = lfn[si++];					/* Get an LFN char */
   1702          		if (!w) break;					/* Break on end of the LFN */
   1703          		if (w == ' ' || (w == '.' && si != di)) {	/* Remove spaces and dots */
   1704          			cf |= NS_LOSS | NS_LFN; continue;
   1705          		}
   1706          
   1707          		if (i >= ni || si == di) {		/* Extension or end of SFN */
   1708          			if (ni == 11) {				/* Long extension */
   1709          				cf |= NS_LOSS | NS_LFN; break;
   1710          			}
   1711          			if (si != di) cf |= NS_LOSS | NS_LFN;	/* Out of 8.3 format */
   1712          			if (si > di) break;			/* No extension */
   1713          			si = di; i = 8; ni = 11;	/* Enter extension section */
   1714          			b <<= 2; continue;
   1715          		}
   1716          
   1717          		if (w >= 0x80) {				/* Non ASCII char */
   1718          #ifdef _EXCVT
   1719          			w = ff_convert(w, 0);		/* Unicode -> OEM code */
   1720          			if (w) w = excvt[w - 0x80];	/* Convert extended char to upper (SBCS) */
   1721          #else
   1722          			w = ff_convert(ff_wtoupper(w), 0);	/* Upper converted Unicode -> OEM code */
   1723          #endif
   1724          			cf |= NS_LFN;				/* Force create LFN entry */
   1725          		}
   1726          
   1727          		if (_DF1S && w >= 0x100) {		/* Double byte char (always false on SBCS cfg) */
   1728          			if (i >= ni - 1) {
   1729          				cf |= NS_LOSS | NS_LFN; i = ni; continue;
   1730          			}
   1731          			dj->fn[i++] = (BYTE)(w >> 8);
   1732          		} else {						/* Single byte char */
   1733          			if (!w || chk_chr("+,;=[]", w)) {	/* Replace illegal chars for SFN */
   1734          				w = '_'; cf |= NS_LOSS | NS_LFN;/* Lossy conversion */
   1735          			} else {
   1736          				if (IsUpper(w)) {		/* ASCII large capital */
   1737          					b |= 2;
   1738          				} else {
   1739          					if (IsLower(w)) {	/* ASCII small capital */
   1740          						b |= 1; w -= 0x20;
   1741          					}
   1742          				}
   1743          			}
   1744          		}
   1745          		dj->fn[i++] = (BYTE)w;
   1746          	}
   1747          
   1748          	if (dj->fn[0] == DDE) dj->fn[0] = NDDE;	/* If the first char collides with deleted mark, replace it with 0x05 */
   1749          
   1750          	if (ni == 8) b <<= 2;
   1751          	if ((b & 0x0C) == 0x0C || (b & 0x03) == 0x03)	/* Create LFN entry when there are composite capitals */
   1752          		cf |= NS_LFN;
   1753          	if (!(cf & NS_LFN)) {						/* When LFN is in 8.3 format without extended char, NT flags are created */
   1754          		if ((b & 0x03) == 0x01) cf |= NS_EXT;	/* NT flag (Extension has only small capital) */
   1755          		if ((b & 0x0C) == 0x04) cf |= NS_BODY;	/* NT flag (Filename has only small capital) */
   1756          	}
   1757          
   1758          	dj->fn[NS] = cf;	/* SFN is created */
   1759          
   1760          	return FR_OK;
   1761          
   1762          
   1763          #else	/* Non-LFN configuration */
   1764          	BYTE b, c, d, *sfn;
   1765          	UINT ni, si, i;
   1766          	const char *p;
   1767          
   1768          	/* Create file name in directory form */
   1769          	for (p = *path; *p == '/' || *p == '\\'; p++) ;	/* Strip duplicated separator */
   1770          	sfn = dj->fn;
   1771          	mem_set(sfn, ' ', 11);
   1772          	si = i = b = 0; ni = 8;
   1773          #if _FS_RPATH
   1774          	if (p[si] == '.') { /* Is this a dot entry? */
   1775          		for (;;) {
   1776          			c = (BYTE)p[si++];
   1777          			if (c != '.' || si >= 3) break;
   1778          			sfn[i++] = c;
   1779          		}
   1780          		if (c != '/' && c != '\\' && c > ' ') return FR_INVALID_NAME;
   1781          		*path = &p[si];									/* Return pointer to the next segment */
   1782          		sfn[NS] = (c <= ' ') ? NS_LAST | NS_DOT : NS_DOT;	/* Set last segment flag if end of path */
   1783          		return FR_OK;
   1784          	}
   1785          #endif
   1786          	for (;;) {
   1787          		c = (BYTE)p[si++];
   1788          		if (c <= ' ' || c == '/' || c == '\\') break;	/* Break on end of segment */
   1789          		if (c == '.' || i >= ni) {
   1790          			if (ni != 8 || c != '.') return FR_INVALID_NAME;
   1791          			i = 8; ni = 11;
   1792          			b <<= 2; continue;
   1793          		}
   1794          		if (c >= 0x80) {				/* Extended char? */
   1795          			b |= 3;						/* Eliminate NT flag */
   1796          #ifdef _EXCVT
   1797          			c = excvt[c-0x80];			/* Upper conversion (SBCS) */
   1798          #else
   1799          #if !_DF1S	/* ASCII only cfg */
   1800          			return FR_INVALID_NAME;
   1801          #endif
   1802          #endif
   1803          		}
   1804          		if (IsDBCS1(c)) {				/* Check if it is a DBC 1st byte (always false on SBCS cfg) */
   1805          			d = (BYTE)p[si++];			/* Get 2nd byte */
   1806          			if (!IsDBCS2(d) || i >= ni - 1)	/* Reject invalid DBC */
   1807          				return FR_INVALID_NAME;
   1808          			sfn[i++] = c;
   1809          			sfn[i++] = d;
   1810          		} else {						/* Single byte code */
   1811          			if (chk_chr("\"*+,:;<=>\?[]|\x7F", c))	/* Reject illegal chrs for SFN */
   1812          				return FR_INVALID_NAME;
   1813          			if (IsUpper(c)) {			/* ASCII large capital? */
   1814          				b |= 2;
   1815          			} else {
   1816          				if (IsLower(c)) {		/* ASCII small capital? */
   1817          					b |= 1; c -= 0x20;
   1818          				}
   1819          			}
   1820          			sfn[i++] = c;
   1821          		}
   1822          	}
   1823          	*path = &p[si];						/* Return pointer to the next segment */
   1824          	c = (c <= ' ') ? NS_LAST : 0;		/* Set last segment flag if end of path */
   1825          
   1826          	if (!i) return FR_INVALID_NAME;		/* Reject nul string */
   1827          	if (sfn[0] == DDE) sfn[0] = NDDE;	/* When first char collides with DDE, replace it with 0x05 */
   1828          
   1829          	if (ni == 8) b <<= 2;
   1830          	if ((b & 0x03) == 0x01) c |= NS_EXT;	/* NT flag (Name extension has only small capital) */
   1831          	if ((b & 0x0C) == 0x04) c |= NS_BODY;	/* NT flag (Name body has only small capital) */
   1832          
   1833          	sfn[NS] = c;		/* Store NT flag, File name is created */
   1834          
   1835          	return FR_OK;
   1836          #endif
   1837          }
