   1924          /*-----------------------------------------------------------------------*/
   1925          /* Follow a file path                                                    */
   1926          /*-----------------------------------------------------------------------*/
   1927          
   1928          static
   1929          FRESULT follow_path (	/* FR_OK(0): successful, !=0: error code */
   1930          	DIR *dj,			/* Directory object to return last directory and found object */
   1931          	const TCHAR *path	/* Full-path string to find a file or directory */
   1932          )
   1933          {
   1934          	FRESULT res;
   1935          	BYTE *dir, ns;
   1936          
   1937          
   1938          #if _FS_RPATH
   1939          	if (*path == '/' || *path == '\\') { /* There is a heading separator */
   1940          		path++;	dj->sclust = 0;		/* Strip it and start from the root dir */
   1941          	} else {							/* No heading separator */
   1942          		dj->sclust = dj->fs->cdir;	/* Start from the current dir */
   1943          	}
   1944          #else
   1945          	if (*path == '/' || *path == '\\')	/* Strip heading separator if exist */
   1946          		path++;
   1947          	dj->sclust = 0;						/* Start from the root dir */
   1948          #endif
   1949          
   1950          	if ((UINT)*path < ' ') {			/* Nul path means the start directory itself */
   1951          		res = dir_sdi(dj, 0);
   1952          		dj->dir = 0;
   1953          
   1954          	} else {							/* Follow path */
   1955          		for (;;) {
   1956          			res = create_name(dj, &path);	/* Get a segment */
   1957          			if (res != FR_OK) break;
   1958          			res = dir_find(dj);				/* Find it */
   1959          			ns = *(dj->fn+NS);
   1960          			if (res != FR_OK) {				/* Failed to find the object */
   1961          				if (res != FR_NO_FILE) break;	/* Abort if any hard error occured */
   1962          				/* Object not found */
   1963          				if (_FS_RPATH && (ns & NS_DOT)) {	/* If dot entry is not exit */
   1964          					dj->sclust = 0; dj->dir = 0;	/* It is the root dir */
   1965          					res = FR_OK;
   1966          					if (!(ns & NS_LAST)) continue;
   1967          				} else {							/* Could not find the object */
   1968          					if (!(ns & NS_LAST)) res = FR_NO_PATH;
   1969          				}
   1970          				break;
   1971          			}
   1972          			if (ns & NS_LAST) break;			/* Last segment match. Function completed. */
   1973          			dir = dj->dir;						/* There is next segment. Follow the sub directory */
   1974          			if (!(dir[DIR_Attr] & AM_DIR)) {	/* Cannot follow because it is a file */
   1975          				res = FR_NO_PATH; break;
   1976          			}
   1977          			dj->sclust = LD_CLUST(dir);
   1978          		}
   1979          	}
   1980          
   1981          	return res;
   1982          }
