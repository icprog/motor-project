   3083          /*-----------------------------------------------------------------------*/
   3084          /* Get Number of Free Clusters                                           */
   3085          /*-----------------------------------------------------------------------*/
   3086          
   3087          FRESULT f_getfree (
   3088          	const TCHAR *path,	/* Pointer to the logical drive number (root dir) */
   3089          	DWORD *nclst,		/* Pointer to the variable to return number of free clusters */
   3090          	FATFS **fatfs		/* Pointer to pointer to corresponding file system object to return */
   3091          )
   3092          {
   3093          	FRESULT res;
   3094          	DWORD n, clst, sect, stat;
   3095          	UINT i;
   3096          	BYTE fat, *p;
   3097          
   3098          
   3099          	/* Get drive number */
   3100          	res = chk_mounted(&path, fatfs, 0);
   3101          	if (res == FR_OK) {
   3102          		/* If free_clust is valid, return it without full cluster scan */
   3103          		if ((*fatfs)->free_clust <= (*fatfs)->n_fatent - 2) {
   3104          			*nclst = (*fatfs)->free_clust;
   3105          		} else {
   3106          			/* Get number of free clusters */
   3107          			fat = (*fatfs)->fs_type;
   3108          			n = 0;
   3109          			if (fat == FS_FAT12) {
   3110          				clst = 2;
   3111          				do {
   3112          					stat = get_fat(*fatfs, clst);
   3113          					if (stat == 0xFFFFFFFF) { res = FR_DISK_ERR; break; }
   3114          					if (stat == 1) { res = FR_INT_ERR; break; }
   3115          					if (stat == 0) n++;
   3116          				} while (++clst < (*fatfs)->n_fatent);
   3117          			} else {
   3118          				clst = (*fatfs)->n_fatent;
   3119          				sect = (*fatfs)->fatbase;
   3120          				i = 0; p = 0;
   3121          				do {
   3122          					if (!i) {
   3123          						res = move_window(*fatfs, sect++);
   3124          						if (res != FR_OK) break;
   3125          						p = (*fatfs)->win;
   3126          						i = SS(*fatfs);
   3127          					}
   3128          					if (fat == FS_FAT16) {
   3129          						if (LD_WORD(p) == 0) n++;
   3130          						p += 2; i -= 2;
   3131          					} else {
   3132          						if ((LD_DWORD(p) & 0x0FFFFFFF) == 0) n++;
   3133          						p += 4; i -= 4;
   3134          					}
   3135          				} while (--clst);
   3136          			}
   3137          			(*fatfs)->free_clust = n;
   3138          			if (fat == FS_FAT32) (*fatfs)->fsi_flag = 1;
   3139          			*nclst = n;
   3140          		}
   3141          	}
   3142          	LEAVE_FF(*fatfs, res);
   3143          }
