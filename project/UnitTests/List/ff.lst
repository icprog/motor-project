   2995          				dj->id = dj->fs->id;
   2996          				res = dir_sdi(dj, 0);			/* Rewind dir */
   2997          			}
   2998          		}
   2999          		if (res == FR_NO_FILE) res = FR_NO_PATH;
   3000          	}
   3001          
   3002          	LEAVE_FF(dj->fs, res);
   3003          }
