   3264          /*-----------------------------------------------------------------------*/
   3265          /* Create a Directory                                                    */
   3266          /*-----------------------------------------------------------------------*/
   3267          
   3268          FRESULT f_mkdir (
   3269          	const TCHAR *path		/* Pointer to the directory path */
   3270          )
   3271          {
   3272          	FRESULT res;
   3273          	DIR dj;
   3274          	BYTE *dir, n;
   3275          	DWORD dsc, dcl, pcl, tim = get_fattime();
   3276          	DEF_NAMEBUF;
   3277          
   3278          
   3279          	res = chk_mounted(&path, &dj.fs, 1);
   3280          	if (res == FR_OK) {
   3281          		INIT_BUF(dj);
   3282          		res = follow_path(&dj, path);			/* Follow the file path */
   3283          		if (res == FR_OK) res = FR_EXIST;		/* Any object with same name is already existing */
   3284          		if (_FS_RPATH && res == FR_NO_FILE && (dj.fn[NS] & NS_DOT))
   3285          			res = FR_INVALID_NAME;
   3286          		if (res == FR_NO_FILE) {				/* Can create a new directory */
   3287          			dcl = create_chain(dj.fs, 0);		/* Allocate a cluster for the new directory table */
   3288          			res = FR_OK;
   3289          			if (dcl == 0) res = FR_DENIED;		/* No space to allocate a new cluster */
   3290          			if (dcl == 1) res = FR_INT_ERR;
   3291          			if (dcl == 0xFFFFFFFF) res = FR_DISK_ERR;
   3292          			if (res == FR_OK)					/* Flush FAT */
   3293          				res = move_window(dj.fs, 0);
   3294          			if (res == FR_OK) {					/* Initialize the new directory table */
   3295          				dsc = clust2sect(dj.fs, dcl);
   3296          				dir = dj.fs->win;
   3297          				mem_set(dir, 0, SS(dj.fs));
   3298          				mem_set(dir+DIR_Name, ' ', 8+3);	/* Create "." entry */
   3299          				dir[DIR_Name] = '.';
   3300          				dir[DIR_Attr] = AM_DIR;
   3301          				ST_DWORD(dir+DIR_WrtTime, tim);
   3302          				ST_CLUST(dir, dcl);
   3303          				mem_cpy(dir+SZ_DIR, dir, SZ_DIR); 	/* Create ".." entry */
   3304          				dir[33] = '.'; pcl = dj.sclust;
   3305          				if (dj.fs->fs_type == FS_FAT32 && pcl == dj.fs->dirbase)
   3306          					pcl = 0;
   3307          				ST_CLUST(dir+SZ_DIR, pcl);
   3308          				for (n = dj.fs->csize; n; n--) {	/* Write dot entries and clear following sectors */
   3309          					dj.fs->winsect = dsc++;
   3310          					dj.fs->wflag = 1;
   3311          					res = move_window(dj.fs, 0);
   3312          					if (res != FR_OK) break;
   3313          					mem_set(dir, 0, SS(dj.fs));
   3314          				}
   3315          			}
   3316          			if (res == FR_OK) res = dir_register(&dj);	/* Register the object to the directoy */
   3317          			if (res != FR_OK) {
   3318          				remove_chain(dj.fs, dcl);			/* Could not register, remove cluster chain */
   3319          			} else {
   3320          				dir = dj.dir;
   3321          				dir[DIR_Attr] = AM_DIR;				/* Attribute */
   3322          				ST_DWORD(dir+DIR_WrtTime, tim);		/* Created time */
   3323          				ST_CLUST(dir, dcl);					/* Table start cluster */
   3324          				dj.fs->wflag = 1;
   3325          				res = sync(dj.fs);
   3326          			}
   3327          		}
   3328          		FREE_BUF();
   3329          	}
   3330          
   3331          	LEAVE_FF(dj.fs, res);
   3332          }
