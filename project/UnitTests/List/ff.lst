   1843          /* Get file information from directory entry                             */
   1844          /*-----------------------------------------------------------------------*/
   1845          #if _FS_MINIMIZE <= 1
   1846          static
   1847          void get_fileinfo (		/* No return code */
   1848          	DIR *dj,			/* Pointer to the directory object */
   1849          	FILINFO *fno	 	/* Pointer to the file information to be filled */
   1850          )
   1851          {
   1852          	UINT i;
   1853          	BYTE nt, *dir;
   1854          	TCHAR *p, c;
   1855          
   1856          
   1857          	p = fno->fname;
   1858          	if (dj->sect) {
   1859          		dir = dj->dir;
   1860          		nt = dir[DIR_NTres];		/* NT flag */
   1861          		for (i = 0; i < 8; i++) {	/* Copy name body */
   1862          			c = dir[i];
   1863          			if (c == ' ') break;
   1864          			if (c == NDDE) c = (TCHAR)DDE;
   1865          			if (_USE_LFN && (nt & NS_BODY) && IsUpper(c)) c += 0x20;
   1866          #if _LFN_UNICODE
   1867          			if (IsDBCS1(c) && i < 7 && IsDBCS2(dir[i+1]))
   1868          				c = (c << 8) | dir[++i];
   1869          			c = ff_convert(c, 1);
   1870          			if (!c) c = '?';
   1871          #endif
   1872          			*p++ = c;
   1873          		}
   1874          		if (dir[8] != ' ') {		/* Copy name extension */
   1875          			*p++ = '.';
   1876          			for (i = 8; i < 11; i++) {
   1877          				c = dir[i];
   1878          				if (c == ' ') break;
   1879          				if (_USE_LFN && (nt & NS_EXT) && IsUpper(c)) c += 0x20;
   1880          #if _LFN_UNICODE
   1881          				if (IsDBCS1(c) && i < 10 && IsDBCS2(dir[i+1]))
   1882          					c = (c << 8) | dir[++i];
   1883          				c = ff_convert(c, 1);
   1884          				if (!c) c = '?';
   1885          #endif
   1886          				*p++ = c;
   1887          			}
   1888          		}
   1889          		fno->fattrib = dir[DIR_Attr];				/* Attribute */
   1890          		fno->fsize = LD_DWORD(dir+DIR_FileSize);	/* Size */
   1891          		fno->fdate = LD_WORD(dir+DIR_WrtDate);		/* Date */
   1892          		fno->ftime = LD_WORD(dir+DIR_WrtTime);		/* Time */
   1893          	}
   1894          	*p = 0;		/* Terminate SFN str by a \0 */
   1895          
   1896          #if _USE_LFN
   1897          	if (fno->lfname && fno->lfsize) {
   1898          		TCHAR *tp = fno->lfname;
   1899          		WCHAR w, *lfn;
   1900          
   1901          		i = 0;
   1902          		if (dj->sect && dj->lfn_idx != 0xFFFF) {/* Get LFN if available */
   1903          			lfn = dj->lfn;
   1904          			while ((w = *lfn++) != 0) {			/* Get an LFN char */
   1905          #if !_LFN_UNICODE
   1906          				w = ff_convert(w, 0);			/* Unicode -> OEM conversion */
   1907          				if (!w) { i = 0; break; }		/* Could not convert, no LFN */
   1908          				if (_DF1S && w >= 0x100)		/* Put 1st byte if it is a DBC (always false on SBCS cfg) */
   1909          					tp[i++] = (TCHAR)(w >> 8);
   1910          #endif
   1911          				if (i >= fno->lfsize - 1) { i = 0; break; }	/* Buffer overflow, no LFN */
   1912          				tp[i++] = (TCHAR)w;
   1913          			}
   1914          		}
   1915          		tp[i] = 0;	/* Terminate the LFN str by a \0 */
   1916          	}
   1917          #endif
   1918          }
   1919          #endif /* _FS_MINIMIZE <= 1 */
