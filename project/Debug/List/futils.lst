     32          UINT8 FutilsDriveNum(char const *pName)
     33          {
     34          	UINT8 drv;
     35          	if (pName[0] == '/')
     36          	{
     37          		drv = pName[1] - '0';
     38          	}
     39          	else if (isdigit(pName[0]))
     40          	{
     41          		drv = pName[0] - '0';
     42          	}
     43          	else
     44          	{
     45          		drv = 0;
     46          	}
     47          	return drv;
     48          }
