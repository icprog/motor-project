     84          BOOL FutilsFilenameIsValid(char const *pName, BOOL wildCardOkay)
     85          {
     86          	char ch;
     87          
     88          	if (!pName || (*pName == F_DELETED_CHAR))
     89          		return FALSE;
     90          
     91          	while ((ch = *pName++) != '\0')
     92          	{
     93          		if (((UINT8)ch < 0x20) || (ch == 0x22) ||
     94          			 (ch == '|') || (ch == '<') || (ch == '>') ||
     95          			 (ch == '/') || (ch == '\\') || (ch == ':'))
     96          		{
     97          			return FALSE;
     98          		}
     99          		if (!wildCardOkay && ((ch == '?') || (ch == '*')))
    100          		{
    101          			return FALSE;
    102          		}
    103          	}
    104          	return TRUE;
    105          }
